async function withSecurity(request, options) {
  /* istanbul ignore next */
  cov_2nwnrol9ci().f[28]++;
  const middleware =
  /* istanbul ignore next */
  (cov_2nwnrol9ci().s[142]++, new SecurityMiddleware(options));
  /* istanbul ignore next */
  cov_2nwnrol9ci().s[143]++;
  return middleware.handle(request);
}
