  (cov_119a3713yf().b[10][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_119a3713yf().b[10][1]++, Object.getOwnPropertyDescriptor));
  /* istanbul ignore next */
  cov_119a3713yf().s[28]++;
  for (var key in obj) {
    /* istanbul ignore next */
    cov_119a3713yf().s[29]++;
    if (
    /* istanbul ignore next */
    (cov_119a3713yf().b[12][0]++, key !== "default") &&
    /* istanbul ignore next */
    (cov_119a3713yf().b[12][1]++, Object.prototype.hasOwnProperty.call(obj, key))) {
      /* istanbul ignore next */
      cov_119a3713yf().b[11][0]++;
      var desc =
      /* istanbul ignore next */
      (cov_119a3713yf().s[30]++, hasPropertyDescriptor ?
      /* istanbul ignore next */
      (cov_119a3713yf().b[13][0]++, Object.getOwnPropertyDescriptor(obj, key)) :
      /* istanbul ignore next */
      (cov_119a3713yf().b[13][1]++, null));
      /* istanbul ignore next */
      cov_119a3713yf().s[31]++;
      if (
      /* istanbul ignore next */
      (cov_119a3713yf().b[15][0]++, desc) && (
      /* istanbul ignore next */
      (cov_119a3713yf().b[15][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_119a3713yf().b[15][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_119a3713yf().b[14][0]++;
        cov_119a3713yf().s[32]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_119a3713yf().b[14][1]++;
        cov_119a3713yf().s[33]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_119a3713yf().b[11][1]++;
    }
  }
  /* istanbul ignore next */
  cov_119a3713yf().s[34]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_119a3713yf().s[35]++;
  if (cache) {
    /* istanbul ignore next */
    cov_119a3713yf().b[16][0]++;
    cov_119a3713yf().s[36]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_119a3713yf().b[16][1]++;
  }
  cov_119a3713yf().s[37]++;
  return newObj;
}
