// Simple in-memory rate limiter (for production, use Redis)
const rateLimitStore =
/* istanbul ignore next */
(cov_2jbamw6pq1().s[48]++, new Map());
function rateLimit(options) {
  /* istanbul ignore next */
  cov_2jbamw6pq1().f[20]++;
  cov_2jbamw6pq1().s[49]++;
  return async request => {
    /* istanbul ignore next */
    cov_2jbamw6pq1().f[21]++;
    const key =
    /* istanbul ignore next */
    (cov_2jbamw6pq1().s[50]++, options.keyGenerator ?
    /* istanbul ignore next */
    (cov_2jbamw6pq1().b[10][0]++, options.keyGenerator(request)) :
    /* istanbul ignore next */
    (cov_2jbamw6pq1().b[10][1]++,
    /* istanbul ignore next */
    (cov_2jbamw6pq1().b[11][0]++, request.ip) ||
    /* istanbul ignore next */
    (cov_2jbamw6pq1().b[11][1]++, 'anonymous')));
    const now =
    /* istanbul ignore next */
    (cov_2jbamw6pq1().s[51]++, Date.now());
    const windowStart =
    /* istanbul ignore next */
    (cov_2jbamw6pq1().s[52]++, now - options.windowMs);
    // Clean up old entries
    /* istanbul ignore next */
    cov_2jbamw6pq1().s[53]++;
    for (const [k, v] of rateLimitStore.entries()) {
      /* istanbul ignore next */
      cov_2jbamw6pq1().s[54]++;
      if (v.resetTime < now) {
        /* istanbul ignore next */
        cov_2jbamw6pq1().b[12][0]++;
        cov_2jbamw6pq1().s[55]++;
        rateLimitStore.delete(k);
      } else
      /* istanbul ignore next */
      {
        cov_2jbamw6pq1().b[12][1]++;
      }
    }
    const current =
    /* istanbul ignore next */
    (cov_2jbamw6pq1().s[56]++, rateLimitStore.get(key));
    /* istanbul ignore next */
    cov_2jbamw6pq1().s[57]++;
    if (
    /* istanbul ignore next */
    (cov_2jbamw6pq1().b[14][0]++, !current) ||
    /* istanbul ignore next */
    (cov_2jbamw6pq1().b[14][1]++, current.resetTime < now)) {
      /* istanbul ignore next */
      cov_2jbamw6pq1().b[13][0]++;
      cov_2jbamw6pq1().s[58]++;
      rateLimitStore.set(key, {
        count: 1,
        resetTime: now + options.windowMs
      });
      /* istanbul ignore next */
      cov_2jbamw6pq1().s[59]++;
      return true;
    } else
    /* istanbul ignore next */
    {
      cov_2jbamw6pq1().b[13][1]++;
    }
    cov_2jbamw6pq1().s[60]++;
    if (current.count >= options.max) {
      /* istanbul ignore next */
      cov_2jbamw6pq1().b[15][0]++;
      cov_2jbamw6pq1().s[61]++;
      throw new ApiException('Too many requests', 429, 'RATE_LIMIT_EXCEEDED');
    } else
    /* istanbul ignore next */
    {
      cov_2jbamw6pq1().b[15][1]++;
    }
    cov_2jbamw6pq1().s[62]++;
    current.count++;
    /* istanbul ignore next */
    cov_2jbamw6pq1().s[63]++;
    return true;
  };
}
function validatePagination(request) {
  /* istanbul ignore next */
  cov_2jbamw6pq1().f[22]++;
  const {
    searchParams
  } =
  /* istanbul ignore next */
  (cov_2jbamw6pq1().s[64]++, new URL(request.url));
  const page =
  /* istanbul ignore next */
  (cov_2jbamw6pq1().s[65]++, Math.max(1, parseInt(
  /* istanbul ignore next */
  (cov_2jbamw6pq1().b[16][0]++, searchParams.get('page')) ||
  /* istanbul ignore next */
  (cov_2jbamw6pq1().b[16][1]++, '1'))));
  const limit =
  /* istanbul ignore next */
  (cov_2jbamw6pq1().s[66]++, Math.min(100, Math.max(1, parseInt(
  /* istanbul ignore next */
  (cov_2jbamw6pq1().b[17][0]++, searchParams.get('limit')) ||
  /* istanbul ignore next */
  (cov_2jbamw6pq1().b[17][1]++, '20')))));
  const offset =
  /* istanbul ignore next */
  (cov_2jbamw6pq1().s[67]++, (page - 1) * limit);
  /* istanbul ignore next */
  cov_2jbamw6pq1().s[68]++;
  return {
    page,
    limit,
    offset
  };
}
function validateSortOrder(request, allowedFields) {
  /* istanbul ignore next */
  cov_2jbamw6pq1().f[23]++;
  const {
    searchParams
  } =
  /* istanbul ignore next */
  (cov_2jbamw6pq1().s[69]++, new URL(request.url));
  const sortBy =
  /* istanbul ignore next */
  (cov_2jbamw6pq1().s[70]++,
  /* istanbul ignore next */
  (cov_2jbamw6pq1().b[18][0]++, searchParams.get('sortBy')) ||
  /* istanbul ignore next */
  (cov_2jbamw6pq1().b[18][1]++, 'created_at'));
  const sortOrder =
  /* istanbul ignore next */
  (cov_2jbamw6pq1().s[71]++,
  /* istanbul ignore next */
  (cov_2jbamw6pq1().b[19][0]++, searchParams.get('sortOrder')) ||
  /* istanbul ignore next */
  (cov_2jbamw6pq1().b[19][1]++, 'desc'));
  /* istanbul ignore next */
  cov_2jbamw6pq1().s[72]++;
  if (!allowedFields.includes(sortBy)) {
    /* istanbul ignore next */
    cov_2jbamw6pq1().b[20][0]++;
    cov_2jbamw6pq1().s[73]++;
    throw new ApiException(`Invalid sort field. Allowed: ${allowedFields.join(', ')}`, 400, 'INVALID_SORT_FIELD');
  } else
  /* istanbul ignore next */
  {
    cov_2jbamw6pq1().b[20][1]++;
  }
  cov_2jbamw6pq1().s[74]++;
  if (!['asc', 'desc'].includes(sortOrder)) {
    /* istanbul ignore next */
    cov_2jbamw6pq1().b[21][0]++;
    cov_2jbamw6pq1().s[75]++;
    throw new ApiException('Invalid sort order. Must be "asc" or "desc"', 400, 'INVALID_SORT_ORDER');
  } else
  /* istanbul ignore next */
  {
    cov_2jbamw6pq1().b[21][1]++;
  }
  cov_2jbamw6pq1().s[76]++;
  return {
    sortBy,
    sortOrder,
    ascending: sortOrder === 'asc'
  };
}
