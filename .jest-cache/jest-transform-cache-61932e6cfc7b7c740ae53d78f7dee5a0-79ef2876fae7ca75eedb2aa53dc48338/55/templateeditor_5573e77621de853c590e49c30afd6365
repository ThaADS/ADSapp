  /* istanbul ignore next */
  cov_1gkvug5sr6().s[17]++;
  if (
  /* istanbul ignore next */
  (cov_1gkvug5sr6().b[7][0]++, cache) &&
  /* istanbul ignore next */
  (cov_1gkvug5sr6().b[7][1]++, cache.has(obj))) {
    /* istanbul ignore next */
    cov_1gkvug5sr6().b[6][0]++;
    cov_1gkvug5sr6().s[18]++;
    return cache.get(obj);
  } else
  /* istanbul ignore next */
  {
    cov_1gkvug5sr6().b[6][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_1gkvug5sr6().s[19]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_1gkvug5sr6().s[20]++,
  /* istanbul ignore next */
  (cov_1gkvug5sr6().b[8][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_1gkvug5sr6().b[8][1]++, Object.getOwnPropertyDescriptor));
  /* istanbul ignore next */
  cov_1gkvug5sr6().s[21]++;
  for (var key in obj) {
    /* istanbul ignore next */
    cov_1gkvug5sr6().s[22]++;
    if (
    /* istanbul ignore next */
    (cov_1gkvug5sr6().b[10][0]++, key !== "default") &&
    /* istanbul ignore next */
    (cov_1gkvug5sr6().b[10][1]++, Object.prototype.hasOwnProperty.call(obj, key))) {
      /* istanbul ignore next */
      cov_1gkvug5sr6().b[9][0]++;
      var desc =
      /* istanbul ignore next */
      (cov_1gkvug5sr6().s[23]++, hasPropertyDescriptor ?
      /* istanbul ignore next */
      (cov_1gkvug5sr6().b[11][0]++, Object.getOwnPropertyDescriptor(obj, key)) :
      /* istanbul ignore next */
      (cov_1gkvug5sr6().b[11][1]++, null));
      /* istanbul ignore next */
      cov_1gkvug5sr6().s[24]++;
      if (
      /* istanbul ignore next */
      (cov_1gkvug5sr6().b[13][0]++, desc) && (
      /* istanbul ignore next */
      (cov_1gkvug5sr6().b[13][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_1gkvug5sr6().b[13][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_1gkvug5sr6().b[12][0]++;
        cov_1gkvug5sr6().s[25]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_1gkvug5sr6().b[12][1]++;
        cov_1gkvug5sr6().s[26]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_1gkvug5sr6().b[9][1]++;
    }
  }
  /* istanbul ignore next */
  cov_1gkvug5sr6().s[27]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_1gkvug5sr6().s[28]++;
  if (cache) {
    /* istanbul ignore next */
    cov_1gkvug5sr6().b[14][0]++;
    cov_1gkvug5sr6().s[29]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_1gkvug5sr6().b[14][1]++;
  }
  cov_1gkvug5sr6().s[30]++;
  return newObj;
}
