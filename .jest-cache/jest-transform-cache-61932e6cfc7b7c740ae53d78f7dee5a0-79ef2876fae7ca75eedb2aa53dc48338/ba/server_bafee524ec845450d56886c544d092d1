        isValid: true,
        sanitizedValue: value
      };
    } else
    /* istanbul ignore next */
    {
      cov_2l8m2uqsed().b[19][1]++;
    }
    cov_2l8m2uqsed().s[64]++;
    if (typeof value === 'string') {
      /* istanbul ignore next */
      cov_2l8m2uqsed().b[20][0]++;
      const lower =
      /* istanbul ignore next */
      (cov_2l8m2uqsed().s[65]++, value.toLowerCase());
      /* istanbul ignore next */
      cov_2l8m2uqsed().s[66]++;
      if (
      /* istanbul ignore next */
      (cov_2l8m2uqsed().b[22][0]++, lower === 'true') ||
      /* istanbul ignore next */
      (cov_2l8m2uqsed().b[22][1]++, lower === '1')) {
        /* istanbul ignore next */
        cov_2l8m2uqsed().b[21][0]++;
        cov_2l8m2uqsed().s[67]++;
        return {
          isValid: true,
          sanitizedValue: true
        };
      } else
      /* istanbul ignore next */
      {
        cov_2l8m2uqsed().b[21][1]++;
      }
      cov_2l8m2uqsed().s[68]++;
      if (
      /* istanbul ignore next */
      (cov_2l8m2uqsed().b[24][0]++, lower === 'false') ||
      /* istanbul ignore next */
      (cov_2l8m2uqsed().b[24][1]++, lower === '0')) {
        /* istanbul ignore next */
        cov_2l8m2uqsed().b[23][0]++;
        cov_2l8m2uqsed().s[69]++;
        return {
          isValid: true,
          sanitizedValue: false
        };
      } else
      /* istanbul ignore next */
      {
        cov_2l8m2uqsed().b[23][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_2l8m2uqsed().b[20][1]++;
