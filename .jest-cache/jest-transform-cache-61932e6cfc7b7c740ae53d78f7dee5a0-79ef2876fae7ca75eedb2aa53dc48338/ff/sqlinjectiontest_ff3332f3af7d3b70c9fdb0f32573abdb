// ERROR-BASED INJECTION TESTS
// ============================================================================
(0, _globals.describe)('Error-Based Injection Prevention', ()=>{
    (0, _globals.it)('should not reveal database information through errors', async ()=>{
        for (const payload of SQL_INJECTION_PAYLOADS.errorBased){
            const result = await (0, _securerpc.secureRpc)(supabase, 'get_tenant_by_domain', {
                domain_name: payload
            });
            (0, _globals.expect)(result.error).toBeDefined();
            // Error message should not contain database information
            (0, _globals.expect)(result.error?.message).not.toContain('version');
            (0, _globals.expect)(result.error?.message).not.toContain('PostgreSQL');
        }
    });
});
// ============================================================================
// RATE LIMITING TESTS
// ============================================================================
(0, _globals.describe)('Rate Limiting', ()=>{
    (0, _globals.it)('should enforce rate limits on repeated calls', async ()=>{
        const functionName = 'is_super_admin';
        const calls = [];
        // Make more calls than the rate limit allows
        for(let i = 0; i < 150; i++){
            calls.push((0, _securerpc.secureRpc)(supabase, functionName, {
                user_id: testUserId
            }));
        }
        const results = await Promise.all(calls);
        const rateLimitErrors = results.filter((r)=>r.error?.message.includes('Rate limit exceeded'));
        (0, _globals.expect)(rateLimitErrors.length).toBeGreaterThan(0);
    });
});
// ============================================================================
// AUDIT LOGGING TESTS
// ============================================================================
(0, _globals.describe)('Audit Logging', ()=>{
    (0, _globals.it)('should log failed injection attempts', async ()=>{
        await (0, _securerpc.secureRpc)(supabase, 'get_conversation_unread_count', {
            conv_id: "'; DROP TABLE messages--"
        });
        const { data: auditLogs } = await supabase.from('rpc_function_audit_log').select('*').eq('function_name', 'get_conversation_unread_count').eq('success', false).order('called_at', {
            ascending: false
        }).limit(1);
        (0, _globals.expect)(auditLogs).toBeDefined();
        (0, _globals.expect)(auditLogs?.length).toBeGreaterThan(0);
    });
    (0, _globals.it)('should sanitize parameters in audit logs', async ()=>{
        await (0, _securerpc.secureRpc)(supabase, 'check_usage_limits', {
            org_id: testOrgId,
            limit_type_param: "messages' OR '1'='1"
        });
        const { data: auditLogs } = await supabase.from('rpc_function_audit_log').select('*').eq('function_name', 'check_usage_limits').order('called_at', {
            ascending: false
        }).limit(1);
        (0, _globals.expect)(auditLogs?.[0]?.parameters).toBeDefined();
    });
});
// ============================================================================
// INTEGRATION TESTS
// ============================================================================
(0, _globals.describe)('End-to-End SQL Injection Prevention', ()=>{
    (0, _globals.it)('should protect full request flow', async ()=>{
        // Simulate a complete user flow with injection attempts
        const maliciousUserId = "'; DROP TABLE profiles--";
        const maliciousOrgId = "' OR '1'='1";
        // All these should fail safely
        const results = await Promise.all([
            (0, _securerpc.secureRpc)(supabase, 'is_super_admin', {
                user_id: maliciousUserId
            }),
            (0, _securerpc.secureRpc)(supabase, 'check_usage_limits', {
                org_id: maliciousOrgId,
                limit_type_param: 'messages'
            }),
            (0, _securerpc.secureRpc)(supabase, 'get_tenant_by_domain', {
                domain_name: "'; DROP TABLE organizations--"
            })
        ]);
        results.forEach((result)=>{
            (0, _globals.expect)(result.error).toBeDefined();
            (0, _globals.expect)(result.data).toBeNull();
        });
    });
    (0, _globals.it)('should allow legitimate requests', async ()=>{
        const result = await (0, _securerpc.secureRpc)(supabase, 'is_super_admin', {
            user_id: testUserId
        });
        // Should not fail on whitelist or validation
        if (result.error) {
            (0, _globals.expect)(result.error.message).not.toContain('not whitelisted');
            (0, _globals.expect)(result.error.message).not.toContain('validation failed');
        }
    });
});

