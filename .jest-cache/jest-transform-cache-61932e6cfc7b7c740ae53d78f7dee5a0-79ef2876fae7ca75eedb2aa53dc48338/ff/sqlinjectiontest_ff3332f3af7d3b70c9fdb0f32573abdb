                "user; DROP TABLE users@example.com"
            ];
            maliciousEmails.forEach((email)=>{
                const result = _inputvalidation.InputValidator.validateEmail(email);
                (0, _globals.expect)(result.isValid).toBe(false);
            });
        });
    });
