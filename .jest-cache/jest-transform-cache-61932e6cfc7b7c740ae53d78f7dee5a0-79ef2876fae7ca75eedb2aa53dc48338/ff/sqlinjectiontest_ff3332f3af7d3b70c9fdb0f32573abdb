(0, _globals.describe)('Union-Based Injection Prevention', ()=>{
    (0, _globals.it)('should reject UNION SELECT in all parameters', async ()=>{
        for (const payload of SQL_INJECTION_PAYLOADS.unionBased){
            const result = await (0, _securerpc.secureRpc)(supabase, 'get_tenant_by_domain', {
                domain_name: payload
            });
            (0, _globals.expect)(result.error).toBeDefined();
            (0, _globals.expect)(result.data).toBeNull();
        }
    });
    (0, _globals.it)('should not leak data through UNION injection', async ()=>{
        const result = await (0, _securerpc.secureRpc)(supabase, 'check_usage_limits', {
            org_id: testOrgId,
            limit_type_param: "messages' UNION SELECT email, password FROM profiles--"
        });
        (0, _globals.expect)(result.error).toBeDefined();
        (0, _globals.expect)(result.data).toBeNull();
    });
});
