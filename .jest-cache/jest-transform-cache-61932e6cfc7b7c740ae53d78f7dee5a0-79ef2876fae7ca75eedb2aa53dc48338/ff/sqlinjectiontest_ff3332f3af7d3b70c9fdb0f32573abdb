(0, _globals.describe)('Stacked Queries Injection Prevention', ()=>{
    (0, _globals.it)('should reject DROP TABLE attempts', async ()=>{
        const dropAttempts = [
            "'; DROP TABLE users--",
            "test'; DROP TABLE profiles; --",
            "value; DELETE FROM organizations--"
        ];
        for (const payload of dropAttempts){
            const result = await (0, _securerpc.secureRpc)(supabase, 'get_tenant_by_domain', {
                domain_name: payload
            });
            (0, _globals.expect)(result.error).toBeDefined();
            (0, _globals.expect)(result.data).toBeNull();
        }
    });
    (0, _globals.it)('should reject UPDATE injection attempts', async ()=>{
        const result = await (0, _securerpc.secureRpc)(supabase, 'check_usage_limits', {
