  cov_hl3rpb5f6().s[91]++;
  const groupMessagesByDate = messages => {
    /* istanbul ignore next */
    cov_hl3rpb5f6().f[22]++;
    const groups =
    /* istanbul ignore next */
    (cov_hl3rpb5f6().s[92]++, []);
    let currentDate =
    /* istanbul ignore next */
    (cov_hl3rpb5f6().s[93]++, '');
    let currentGroup =
    /* istanbul ignore next */
    (cov_hl3rpb5f6().s[94]++, []);
    /* istanbul ignore next */
    cov_hl3rpb5f6().s[95]++;
    messages.forEach(message => {
      /* istanbul ignore next */
      cov_hl3rpb5f6().f[23]++;
      const messageDate =
      /* istanbul ignore next */
      (cov_hl3rpb5f6().s[96]++, new Date(message.created_at).toDateString());
      /* istanbul ignore next */
      cov_hl3rpb5f6().s[97]++;
      if (messageDate !== currentDate) {
        /* istanbul ignore next */
        cov_hl3rpb5f6().b[27][0]++;
        cov_hl3rpb5f6().s[98]++;
        if (currentGroup.length > 0) {
          /* istanbul ignore next */
          cov_hl3rpb5f6().b[28][0]++;
          cov_hl3rpb5f6().s[99]++;
          groups.push({
            date: currentDate,
            messages: currentGroup
          });
        } else
        /* istanbul ignore next */
        {
          cov_hl3rpb5f6().b[28][1]++;
        }
        cov_hl3rpb5f6().s[100]++;
        currentDate = messageDate;
        /* istanbul ignore next */
        cov_hl3rpb5f6().s[101]++;
        currentGroup = [message];
      } else {
        /* istanbul ignore next */
        cov_hl3rpb5f6().b[27][1]++;
        cov_hl3rpb5f6().s[102]++;
        currentGroup.push(message);
      }
    });
    /* istanbul ignore next */
    cov_hl3rpb5f6().s[103]++;
    if (currentGroup.length > 0) {
      /* istanbul ignore next */
      cov_hl3rpb5f6().b[29][0]++;
      cov_hl3rpb5f6().s[104]++;
      groups.push({
        date: currentDate,
        messages: currentGroup
      });
    } else
    /* istanbul ignore next */
    {
      cov_hl3rpb5f6().b[29][1]++;
    }
    cov_hl3rpb5f6().s[105]++;
    return groups;
  };
  /* istanbul ignore next */
  cov_hl3rpb5f6().s[106]++;
  const formatDateHeader = dateString => {
    /* istanbul ignore next */
    cov_hl3rpb5f6().f[24]++;
    const date =
    /* istanbul ignore next */
    (cov_hl3rpb5f6().s[107]++, new Date(dateString));
    const today =
    /* istanbul ignore next */
    (cov_hl3rpb5f6().s[108]++, new Date());
    const yesterday =
    /* istanbul ignore next */
    (cov_hl3rpb5f6().s[109]++, new Date(today.getTime() - 24 * 60 * 60 * 1000));
    /* istanbul ignore next */
    cov_hl3rpb5f6().s[110]++;
    if (date.toDateString() === today.toDateString()) {
      /* istanbul ignore next */
      cov_hl3rpb5f6().b[30][0]++;
      cov_hl3rpb5f6().s[111]++;
