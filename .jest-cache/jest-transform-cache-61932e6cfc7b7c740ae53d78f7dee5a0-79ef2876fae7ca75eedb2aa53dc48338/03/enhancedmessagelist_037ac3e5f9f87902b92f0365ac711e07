  const messageGroups =
  /* istanbul ignore next */
  (cov_hl3rpb5f6().s[115]++, groupMessagesByDate(messages));
  /* istanbul ignore next */
  cov_hl3rpb5f6().s[116]++;
  return /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
    className: "flex flex-col h-full",
    children: [
    /* istanbul ignore next */
