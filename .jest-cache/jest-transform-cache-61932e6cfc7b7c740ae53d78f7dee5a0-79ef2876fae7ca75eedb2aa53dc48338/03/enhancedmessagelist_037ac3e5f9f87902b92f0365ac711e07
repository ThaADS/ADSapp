}
function EnhancedMessageList({
  conversationId,
  messages,
  currentUserId,
  onMessageRead,
  onLoadMore,
  hasMore,
  loading
}) {
  /* istanbul ignore next */
  cov_hl3rpb5f6().f[11]++;
  const messagesEndRef =
  /* istanbul ignore next */
  (cov_hl3rpb5f6().s[54]++, (0, _react.useRef)(null));
  const mediaHandler =
  /* istanbul ignore next */
  (cov_hl3rpb5f6().s[55]++, new _mediahandler.WhatsAppMediaHandler('', '')) // Would be initialized with proper tokens
  ;
  /* istanbul ignore next */
  cov_hl3rpb5f6().s[56]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_hl3rpb5f6().f[12]++;
    cov_hl3rpb5f6().s[57]++;
    scrollToBottom();
  }, [messages]);
  /* istanbul ignore next */
  cov_hl3rpb5f6().s[58]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_hl3rpb5f6().f[13]++;
    cov_hl3rpb5f6().s[59]++;
    // Mark unread messages as read when they come into view
    markVisibleMessagesAsRead();
  }, [messages, onMessageRead]);
  /* istanbul ignore next */
  cov_hl3rpb5f6().s[60]++;
  const scrollToBottom = () => {
    /* istanbul ignore next */
    cov_hl3rpb5f6().f[14]++;
    cov_hl3rpb5f6().s[61]++;
    messagesEndRef.current?.scrollIntoView({
      behavior: 'smooth'
    });
  };
  /* istanbul ignore next */
  cov_hl3rpb5f6().s[62]++;
  const markVisibleMessagesAsRead = () => {
    /* istanbul ignore next */
    cov_hl3rpb5f6().f[15]++;
    const unreadMessages =
    /* istanbul ignore next */
    (cov_hl3rpb5f6().s[63]++, messages.filter(message => {
      /* istanbul ignore next */
      cov_hl3rpb5f6().f[16]++;
      cov_hl3rpb5f6().s[64]++;
      return /* istanbul ignore next */(cov_hl3rpb5f6().b[21][0]++, !message.is_read) &&
      /* istanbul ignore next */
      (cov_hl3rpb5f6().b[21][1]++, message.sender_type === 'contact');
    }));
    /* istanbul ignore next */
    cov_hl3rpb5f6().s[65]++;
    unreadMessages.forEach(message => {
      /* istanbul ignore next */
      cov_hl3rpb5f6().f[17]++;
      cov_hl3rpb5f6().s[66]++;
      if (onMessageRead) {
        /* istanbul ignore next */
        cov_hl3rpb5f6().b[22][0]++;
        cov_hl3rpb5f6().s[67]++;
        onMessageRead(message.id);
      } else
      /* istanbul ignore next */
      {
        cov_hl3rpb5f6().b[22][1]++;
      }
    });
  };
  /* istanbul ignore next */
  cov_hl3rpb5f6().s[68]++;
  const handleMediaDownload = async mediaId => {
    /* istanbul ignore next */
    cov_hl3rpb5f6().f[18]++;
    cov_hl3rpb5f6().s[69]++;
    try {
      // Create download link
      const message =
      /* istanbul ignore next */
      (cov_hl3rpb5f6().s[70]++, messages.find(m => {
        /* istanbul ignore next */
        cov_hl3rpb5f6().f[19]++;
        cov_hl3rpb5f6().s[71]++;
        return m.media?.id === mediaId;
      }));
      /* istanbul ignore next */
      cov_hl3rpb5f6().s[72]++;
      if (message?.media?.url) {
        /* istanbul ignore next */
        cov_hl3rpb5f6().b[23][0]++;
        const link =
        /* istanbul ignore next */
        (cov_hl3rpb5f6().s[73]++, document.createElement('a'));
        /* istanbul ignore next */
        cov_hl3rpb5f6().s[74]++;
        link.href = message.media.url;
        /* istanbul ignore next */
        cov_hl3rpb5f6().s[75]++;
        link.download = message.media.filename;
        /* istanbul ignore next */
        cov_hl3rpb5f6().s[76]++;
        document.body.appendChild(link);
        /* istanbul ignore next */
        cov_hl3rpb5f6().s[77]++;
        link.click();
        /* istanbul ignore next */
        cov_hl3rpb5f6().s[78]++;
        document.body.removeChild(link);
      } else
      /* istanbul ignore next */
      {
        cov_hl3rpb5f6().b[23][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_hl3rpb5f6().s[79]++;
      console.error('Download failed:', error);
    }
  };
  /* istanbul ignore next */
  cov_hl3rpb5f6().s[80]++;
  const formatTime = timestamp => {
    /* istanbul ignore next */
    cov_hl3rpb5f6().f[20]++;
    const date =
    /* istanbul ignore next */
    (cov_hl3rpb5f6().s[81]++, new Date(timestamp));
    /* istanbul ignore next */
    cov_hl3rpb5f6().s[82]++;
    return date.toLocaleTimeString([], {
      hour: '2-digit',
      minute: '2-digit'
    });
  };
  /* istanbul ignore next */
  cov_hl3rpb5f6().s[83]++;
  const getDeliveryStatus = message => {
    /* istanbul ignore next */
    cov_hl3rpb5f6().f[21]++;
    cov_hl3rpb5f6().s[84]++;
    if (message.sender_type !== 'agent') {
      /* istanbul ignore next */
      cov_hl3rpb5f6().b[24][0]++;
      cov_hl3rpb5f6().s[85]++;
      return null;
    } else
    /* istanbul ignore next */
    {
      cov_hl3rpb5f6().b[24][1]++;
    }
    cov_hl3rpb5f6().s[86]++;
