      /* istanbul ignore next */
      (cov_1ctj4lxpcm().s[57]++, /^[^\s@]+@[^\s@]+\.[^\s@]+$/);
      /* istanbul ignore next */
      cov_1ctj4lxpcm().s[58]++;
      if (!emailRegex.test(email)) {
        /* istanbul ignore next */
        cov_1ctj4lxpcm().b[22][0]++;
        cov_1ctj4lxpcm().s[59]++;
        return _server.NextResponse.json({
          error: 'Invalid email format'
        }, {
          status: 400
        });
      } else
      /* istanbul ignore next */
      {
        cov_1ctj4lxpcm().b[22][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_1ctj4lxpcm().b[21][1]++;
    }
    const supabase =
    /* istanbul ignore next */
    (cov_1ctj4lxpcm().s[60]++, await (0, _server1.createClient)());
    // Check for duplicates
    const {
      data: existingContact
    } =
    /* istanbul ignore next */
    (cov_1ctj4lxpcm().s[61]++, await supabase.from('contacts').select('id, phone_number').eq('organization_id', organizationId).eq('phone_number', phone_number).single());
    /* istanbul ignore next */
    cov_1ctj4lxpcm().s[62]++;
    if (existingContact) {
      /* istanbul ignore next */
      cov_1ctj4lxpcm().b[23][0]++;
      cov_1ctj4lxpcm().s[63]++;
      return _server.NextResponse.json({
        error: 'Contact with this phone number already exists'
      }, {
        status: 409
      });
    } else
    /* istanbul ignore next */
    {
      cov_1ctj4lxpcm().b[23][1]++;
    }
    // Create contact
    const {
      data: contact,
      error
    } =
    /* istanbul ignore next */
    (cov_1ctj4lxpcm().s[64]++, await supabase.from('contacts').insert({
      organization_id: organizationId,
      phone_number,
      whatsapp_id:
      /* istanbul ignore next */
      (cov_1ctj4lxpcm().b[24][0]++, whatsapp_id) ||
      /* istanbul ignore next */
      (cov_1ctj4lxpcm().b[24][1]++, phone_number),
      name,
      email,
      tags,
      notes,
      metadata: {
        ...metadata,
        created_by: userId,
        source: 'manual'
      }
    }).select().single());
    /* istanbul ignore next */
    cov_1ctj4lxpcm().s[65]++;
    if (error) {
      /* istanbul ignore next */
      cov_1ctj4lxpcm().b[25][0]++;
      cov_1ctj4lxpcm().s[66]++;
      throw error;
    } else
    /* istanbul ignore next */
    {
      cov_1ctj4lxpcm().b[25][1]++;
    }
    cov_1ctj4lxpcm().s[67]++;
    return (0, _apiutils.createSuccessResponse)(contact, 201);
  } catch (error) {
    /* istanbul ignore next */
    cov_1ctj4lxpcm().s[68]++;
    console.error('Error creating contact:', error);
    /* istanbul ignore next */
    cov_1ctj4lxpcm().s[69]++;
    return (0, _apiutils.createErrorResponse)(error);
  }
}
async function applySegmentation(query, segment, organizationId) {
  /* istanbul ignore next */
  cov_1ctj4lxpcm().f[8]++;
  cov_1ctj4lxpcm().s[70]++;
  switch (segment) {
    case 'active':
      /* istanbul ignore next */
      cov_1ctj4lxpcm().b[26][0]++;
      // Contacts with messages in last 30 days
      const thirtyDaysAgo =
      /* istanbul ignore next */
      (cov_1ctj4lxpcm().s[71]++, new Date(Date.now() - 30 * 24 * 60 * 60 * 1000).toISOString());
      /* istanbul ignore next */
      cov_1ctj4lxpcm().s[72]++;
      return query.gte('last_message_at', thirtyDaysAgo);
    case 'inactive':
      /* istanbul ignore next */
      cov_1ctj4lxpcm().b[26][1]++;
      // Contacts with no messages in last 30 days
      const thirtyDaysAgoInactive =
      /* istanbul ignore next */
      (cov_1ctj4lxpcm().s[73]++, new Date(Date.now() - 30 * 24 * 60 * 60 * 1000).toISOString());
      /* istanbul ignore next */
      cov_1ctj4lxpcm().s[74]++;
      return query.or(`last_message_at.is.null,last_message_at.lt.${thirtyDaysAgoInactive}`);
    case 'new':
      /* istanbul ignore next */
      cov_1ctj4lxpcm().b[26][2]++;
      // Contacts created in last 7 days
      const sevenDaysAgo =
      /* istanbul ignore next */
      (cov_1ctj4lxpcm().s[75]++, new Date(Date.now() - 7 * 24 * 60 * 60 * 1000).toISOString());
      /* istanbul ignore next */
      cov_1ctj4lxpcm().s[76]++;
      return query.gte('created_at', sevenDaysAgo);
    case 'vip':
      /* istanbul ignore next */
      cov_1ctj4lxpcm().b[26][3]++;
      cov_1ctj4lxpcm().s[77]++;
      // Contacts with VIP tag
      return query.contains('tags', ['vip']);
    case 'blocked':
      /* istanbul ignore next */
      cov_1ctj4lxpcm().b[26][4]++;
      cov_1ctj4lxpcm().s[78]++;
      // Blocked contacts
      return query.eq('is_blocked', true);
    default:
      /* istanbul ignore next */
      cov_1ctj4lxpcm().b[26][5]++;
      cov_1ctj4lxpcm().s[79]++;
      return query;
  }
}
