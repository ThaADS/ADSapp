      timestamp: Date.now(),
      url: window.location.href
    });
  }
  constructor() {
    /* istanbul ignore next */
    cov_z0gptzav9().f[22]++;
    cov_z0gptzav9().s[61]++;
    this.metrics = {};
    /* istanbul ignore next */
    cov_z0gptzav9().s[62]++;
    this.isInitialized = false;
  }
