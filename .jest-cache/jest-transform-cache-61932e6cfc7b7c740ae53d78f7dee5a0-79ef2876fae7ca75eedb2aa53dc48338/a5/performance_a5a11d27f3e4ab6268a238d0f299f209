const performanceMonitor =
/* istanbul ignore next */
(cov_z0gptzav9().s[63]++, PerformanceMonitor.getInstance());
function withPerformanceTracking(fn, name) {
  /* istanbul ignore next */
  cov_z0gptzav9().f[23]++;
  cov_z0gptzav9().s[64]++;
  return (...args) => {
    /* istanbul ignore next */
    cov_z0gptzav9().f[24]++;
    cov_z0gptzav9().s[65]++;
    performanceMonitor.startTiming(name);
    const result =
    /* istanbul ignore next */
    (cov_z0gptzav9().s[66]++, fn(...args));
    /* istanbul ignore next */
    cov_z0gptzav9().s[67]++;
    if (result instanceof Promise) {
      /* istanbul ignore next */
      cov_z0gptzav9().b[16][0]++;
      cov_z0gptzav9().s[68]++;
      return result.finally(() => {
        /* istanbul ignore next */
        cov_z0gptzav9().f[25]++;
        cov_z0gptzav9().s[69]++;
        performanceMonitor.endTiming(name);
      });
    } else {
      /* istanbul ignore next */
      cov_z0gptzav9().b[16][1]++;
      cov_z0gptzav9().s[70]++;
      performanceMonitor.endTiming(name);
      /* istanbul ignore next */
      cov_z0gptzav9().s[71]++;
      return result;
    }
  };
}
function usePerformanceTracking(componentName) {
  /* istanbul ignore next */
  cov_z0gptzav9().f[26]++;
  const startTime =
  /* istanbul ignore next */
  (cov_z0gptzav9().s[72]++, performance.now());
  /* istanbul ignore next */
  cov_z0gptzav9().s[73]++;
  return {
    trackRender: () => {
      /* istanbul ignore next */
      cov_z0gptzav9().f[27]++;
      const renderTime =
      /* istanbul ignore next */
      (cov_z0gptzav9().s[74]++, performance.now() - startTime);
      /* istanbul ignore next */
      cov_z0gptzav9().s[75]++;
      performanceMonitor.sendToCustomAnalytics({
        type: 'component-render',
        component: componentName,
        duration: renderTime,
        timestamp: Date.now(),
        url: window.location.href
      });
    }
  };
}
// Global error handler
/* istanbul ignore next */
cov_z0gptzav9().s[76]++;
if (typeof window !== 'undefined') {
  /* istanbul ignore next */
  cov_z0gptzav9().b[17][0]++;
  cov_z0gptzav9().s[77]++;
  window.addEventListener('error', event => {
    /* istanbul ignore next */
    cov_z0gptzav9().f[28]++;
    cov_z0gptzav9().s[78]++;
    performanceMonitor.trackError(event.error, {
      filename: event.filename,
      lineno: event.lineno,
      colno: event.colno
    });
  });
  /* istanbul ignore next */
  cov_z0gptzav9().s[79]++;
  window.addEventListener('unhandledrejection', event => {
    /* istanbul ignore next */
    cov_z0gptzav9().f[29]++;
    cov_z0gptzav9().s[80]++;
    performanceMonitor.trackError(new Error(event.reason), {
      type: 'unhandled-promise-rejection'
    });
  });
} else
/* istanbul ignore next */
{
  cov_z0gptzav9().b[17][1]++;
}
