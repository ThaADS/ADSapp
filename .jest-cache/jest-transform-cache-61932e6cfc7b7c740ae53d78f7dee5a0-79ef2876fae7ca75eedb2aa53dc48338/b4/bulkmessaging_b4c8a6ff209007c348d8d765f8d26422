  /**
  * Get campaigns for organization
  */
  async getCampaigns(organizationId, options) {
    /* istanbul ignore next */
    cov_3mr15gia3().f[10]++;
    cov_3mr15gia3().s[59]++;
    try {
      let query =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[60]++, this.supabase.from('bulk_campaigns').select('*', {
        count: 'exact'
      }).eq('organization_id', organizationId));
      /* istanbul ignore next */
      cov_3mr15gia3().s[61]++;
      if (options?.status) {
        /* istanbul ignore next */
        cov_3mr15gia3().b[17][0]++;
        cov_3mr15gia3().s[62]++;
        query = query.eq('status', options.status);
      } else
      /* istanbul ignore next */
      {
        cov_3mr15gia3().b[17][1]++;
      }
      cov_3mr15gia3().s[63]++;
      if (options?.type) {
        /* istanbul ignore next */
        cov_3mr15gia3().b[18][0]++;
        cov_3mr15gia3().s[64]++;
        query = query.eq('type', options.type);
      } else
      /* istanbul ignore next */
      {
        cov_3mr15gia3().b[18][1]++;
      }
      const limit =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[65]++,
      /* istanbul ignore next */
      (cov_3mr15gia3().b[19][0]++, options?.limit) ||
      /* istanbul ignore next */
      (cov_3mr15gia3().b[19][1]++, 50));
      const offset =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[66]++,
      /* istanbul ignore next */
      (cov_3mr15gia3().b[20][0]++, options?.offset) ||
      /* istanbul ignore next */
      (cov_3mr15gia3().b[20][1]++, 0));
      /* istanbul ignore next */
      cov_3mr15gia3().s[67]++;
      query = query.range(offset, offset + limit - 1);
      /* istanbul ignore next */
      cov_3mr15gia3().s[68]++;
      query = query.order('created_at', {
        ascending: false
      });
      const {
        data,
        error,
        count
      } =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[69]++, await query);
      /* istanbul ignore next */
      cov_3mr15gia3().s[70]++;
      if (error) {
        /* istanbul ignore next */
        cov_3mr15gia3().b[21][0]++;
        cov_3mr15gia3().s[71]++;
        throw new Error(`Failed to get campaigns: ${error.message}`);
      } else
      /* istanbul ignore next */
      {
        cov_3mr15gia3().b[21][1]++;
      }
      const campaigns =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[72]++,
      /* istanbul ignore next */
      (cov_3mr15gia3().b[22][0]++, data?.map(item => {
        /* istanbul ignore next */
        cov_3mr15gia3().f[11]++;
        cov_3mr15gia3().s[73]++;
        return this.mapToCampaign(item);
      })) ||
      /* istanbul ignore next */
      (cov_3mr15gia3().b[22][1]++, []));
      /* istanbul ignore next */
      cov_3mr15gia3().s[74]++;
      return {
        campaigns,
        total:
        /* istanbul ignore next */
        (cov_3mr15gia3().b[23][0]++, count) ||
        /* istanbul ignore next */
        (cov_3mr15gia3().b[23][1]++, 0),
        hasMore: offset + limit < (
        /* istanbul ignore next */
        (cov_3mr15gia3().b[24][0]++, count) ||
        /* istanbul ignore next */
        (cov_3mr15gia3().b[24][1]++, 0))
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_3mr15gia3().s[75]++;
      throw new Error(`Failed to get campaigns: ${error instanceof Error ?
      /* istanbul ignore next */
      (cov_3mr15gia3().b[25][0]++, error.message) :
      /* istanbul ignore next */
      (cov_3mr15gia3().b[25][1]++, 'Unknown error')}`);
    }
  }
  /**
  * Calculate audience size based on targeting criteria
  */
  async calculateAudienceSize(organizationId, targetAudience) {
    /* istanbul ignore next */
    cov_3mr15gia3().f[12]++;
    cov_3mr15gia3().s[76]++;
    try {
      let query =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[77]++, this.supabase.from('contacts').select('id', {
        count: 'exact',
        head: true
      }).eq('organization_id', organizationId).eq('is_blocked', false));
      // Apply audience filters
      /* istanbul ignore next */
      cov_3mr15gia3().s[78]++;
      switch (targetAudience.type) {
        case 'all':
          /* istanbul ignore next */
          cov_3mr15gia3().b[26][0]++;
          cov_3mr15gia3().s[79]++;
          break;
        case 'tags':
          /* istanbul ignore next */
          cov_3mr15gia3().b[26][1]++;
          cov_3mr15gia3().s[80]++;
          if (
          /* istanbul ignore next */
          (cov_3mr15gia3().b[28][0]++, targetAudience.tags) &&
          /* istanbul ignore next */
          (cov_3mr15gia3().b[28][1]++, targetAudience.tags.length > 0)) {
            /* istanbul ignore next */
            cov_3mr15gia3().b[27][0]++;
            cov_3mr15gia3().s[81]++;
            query = query.overlaps('tags', targetAudience.tags);
          } else
          /* istanbul ignore next */
          {
            cov_3mr15gia3().b[27][1]++;
          }
          cov_3mr15gia3().s[82]++;
          break;
        case 'contacts':
          /* istanbul ignore next */
          cov_3mr15gia3().b[26][2]++;
          cov_3mr15gia3().s[83]++;
          if (
          /* istanbul ignore next */
          (cov_3mr15gia3().b[30][0]++, targetAudience.contactIds) &&
          /* istanbul ignore next */
          (cov_3mr15gia3().b[30][1]++, targetAudience.contactIds.length > 0)) {
            /* istanbul ignore next */
            cov_3mr15gia3().b[29][0]++;
            cov_3mr15gia3().s[84]++;
            query = query.in('id', targetAudience.contactIds);
          } else
          /* istanbul ignore next */
          {
            cov_3mr15gia3().b[29][1]++;
          }
          cov_3mr15gia3().s[85]++;
          break;
        case 'custom':
          /* istanbul ignore next */
          cov_3mr15gia3().b[26][3]++;
          cov_3mr15gia3().s[86]++;
          if (targetAudience.customFilters) {
            /* istanbul ignore next */
            cov_3mr15gia3().b[31][0]++;
            cov_3mr15gia3().s[87]++;
            query = this.applyCustomFilters(query, targetAudience.customFilters);
          } else
          /* istanbul ignore next */
          {
            cov_3mr15gia3().b[31][1]++;
          }
          cov_3mr15gia3().s[88]++;
          break;
      }
      const {
        count,
        error
      } =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[89]++, await query);
      /* istanbul ignore next */
      cov_3mr15gia3().s[90]++;
      if (error) {
        /* istanbul ignore next */
        cov_3mr15gia3().b[32][0]++;
        cov_3mr15gia3().s[91]++;
        throw new Error(`Failed to calculate audience size: ${error.message}`);
      } else
      /* istanbul ignore next */
      {
        cov_3mr15gia3().b[32][1]++;
      }
      cov_3mr15gia3().s[92]++;
      return /* istanbul ignore next */(cov_3mr15gia3().b[33][0]++, count) ||
      /* istanbul ignore next */
      (cov_3mr15gia3().b[33][1]++, 0);
    } catch (error) {
      /* istanbul ignore next */
      cov_3mr15gia3().s[93]++;
      throw new Error(`Failed to calculate audience size: ${error instanceof Error ?
      /* istanbul ignore next */
      (cov_3mr15gia3().b[34][0]++, error.message) :
      /* istanbul ignore next */
      (cov_3mr15gia3().b[34][1]++, 'Unknown error')}`);
    }
  }
  /**
  * Apply custom filters to query
  */
  applyCustomFilters(query, filters) {
    /* istanbul ignore next */
    cov_3mr15gia3().f[13]++;
    cov_3mr15gia3().s[94]++;
    filters.forEach(filter => {
      /* istanbul ignore next */
      cov_3mr15gia3().f[14]++;
      cov_3mr15gia3().s[95]++;
      switch (filter.operator) {
        case 'equals':
          /* istanbul ignore next */
          cov_3mr15gia3().b[35][0]++;
          cov_3mr15gia3().s[96]++;
          query = query.eq(filter.field, filter.value);
          /* istanbul ignore next */
          cov_3mr15gia3().s[97]++;
          break;
        case 'contains':
          /* istanbul ignore next */
          cov_3mr15gia3().b[35][1]++;
          cov_3mr15gia3().s[98]++;
          query = query.like(filter.field, `%${filter.value}%`);
          /* istanbul ignore next */
          cov_3mr15gia3().s[99]++;
          break;
        case 'starts_with':
          /* istanbul ignore next */
          cov_3mr15gia3().b[35][2]++;
          cov_3mr15gia3().s[100]++;
          query = query.like(filter.field, `${filter.value}%`);
          /* istanbul ignore next */
          cov_3mr15gia3().s[101]++;
          break;
        case 'ends_with':
          /* istanbul ignore next */
          cov_3mr15gia3().b[35][3]++;
          cov_3mr15gia3().s[102]++;
          query = query.like(filter.field, `%${filter.value}`);
          /* istanbul ignore next */
          cov_3mr15gia3().s[103]++;
          break;
        case 'before':
          /* istanbul ignore next */
          cov_3mr15gia3().b[35][4]++;
          cov_3mr15gia3().s[104]++;
          query = query.lt(filter.field, filter.value);
          /* istanbul ignore next */
          cov_3mr15gia3().s[105]++;
          break;
        case 'after':
          /* istanbul ignore next */
          cov_3mr15gia3().b[35][5]++;
          cov_3mr15gia3().s[106]++;
          query = query.gt(filter.field, filter.value);
          /* istanbul ignore next */
          cov_3mr15gia3().s[107]++;
          break;
        case 'in':
          /* istanbul ignore next */
          cov_3mr15gia3().b[35][6]++;
          cov_3mr15gia3().s[108]++;
          query = query.in(filter.field, filter.value);
          /* istanbul ignore next */
          cov_3mr15gia3().s[109]++;
          break;
        case 'not_in':
          /* istanbul ignore next */
          cov_3mr15gia3().b[35][7]++;
          cov_3mr15gia3().s[110]++;
          query = query.not(filter.field, 'in', `(${filter.value.join(',')})`);
          /* istanbul ignore next */
          cov_3mr15gia3().s[111]++;
          break;
      }
    });
    /* istanbul ignore next */
    cov_3mr15gia3().s[112]++;
    return query;
  }
  /**
  * Generate message jobs for campaign
  */
  async generateMessageJobs(campaign) {
    /* istanbul ignore next */
    cov_3mr15gia3().f[15]++;
    cov_3mr15gia3().s[113]++;
    try {
      // Get target contacts
      const contacts =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[114]++, await this.getTargetContacts(campaign.organizationId, campaign.targetAudience));
      const jobs =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[115]++, []);
      const now =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[116]++, new Date());
      /* istanbul ignore next */
      cov_3mr15gia3().s[117]++;
      contacts.forEach((contact, index) => {
        /* istanbul ignore next */
        cov_3mr15gia3().f[16]++;
        let scheduledAt =
        /* istanbul ignore next */
        (cov_3mr15gia3().s[118]++, now);
        // Apply rate limiting delays
        /* istanbul ignore next */
        cov_3mr15gia3().s[119]++;
        if (campaign.rateLimiting.enabled) {
          /* istanbul ignore next */
          cov_3mr15gia3().b[36][0]++;
          const delayMinutes =
          /* istanbul ignore next */
          (cov_3mr15gia3().s[120]++, Math.floor(index / (campaign.rateLimiting.messagesPerHour / 60)));
          /* istanbul ignore next */
          cov_3mr15gia3().s[121]++;
          scheduledAt = new Date(now.getTime() + delayMinutes * 60 * 1000);
        } else
        /* istanbul ignore next */
        {
          cov_3mr15gia3().b[36][1]++;
        }
        // Apply campaign scheduling
        cov_3mr15gia3().s[122]++;
        if (
        /* istanbul ignore next */
        (cov_3mr15gia3().b[38][0]++, campaign.scheduling.type === 'scheduled') &&
        /* istanbul ignore next */
        (cov_3mr15gia3().b[38][1]++, campaign.scheduling.scheduledAt)) {
          /* istanbul ignore next */
          cov_3mr15gia3().b[37][0]++;
          const baseScheduledTime =
          /* istanbul ignore next */
          (cov_3mr15gia3().s[123]++, campaign.scheduling.scheduledAt);
          /* istanbul ignore next */
          cov_3mr15gia3().s[124]++;
          scheduledAt = new Date(baseScheduledTime.getTime() + index * 1000); // 1 second intervals
        } else
        /* istanbul ignore next */
        {
          cov_3mr15gia3().b[37][1]++;
        }
        cov_3mr15gia3().s[125]++;
        jobs.push({
          campaignId: campaign.id,
          contactId: contact.id,
          whatsappId: contact.whatsapp_id,
          status: 'pending',
          scheduledAt,
          retryCount: 0,
          maxRetries: 3
        });
      });
      // Insert jobs into database
      const jobsData =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[126]++, jobs.map(job => {
        /* istanbul ignore next */
        cov_3mr15gia3().f[17]++;
        cov_3mr15gia3().s[127]++;
        return {
          id: _crypto.default.randomUUID(),
          campaign_id: job.campaignId,
          contact_id: job.contactId,
          whatsapp_id: job.whatsappId,
          status: job.status,
          scheduled_at: job.scheduledAt,
          retry_count: job.retryCount,
          max_retries: job.maxRetries
        };
      }));
      const {
        data,
        error
      } =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[128]++, await this.supabase.from('bulk_message_jobs').insert(jobsData).select());
      /* istanbul ignore next */
      cov_3mr15gia3().s[129]++;
      if (error) {
        /* istanbul ignore next */
        cov_3mr15gia3().b[39][0]++;
        cov_3mr15gia3().s[130]++;
        throw new Error(`Failed to create message jobs: ${error.message}`);
      } else
      /* istanbul ignore next */
      {
        cov_3mr15gia3().b[39][1]++;
      }
      cov_3mr15gia3().s[131]++;
      return data.map(item => {
        /* istanbul ignore next */
        cov_3mr15gia3().f[18]++;
        cov_3mr15gia3().s[132]++;
        return this.mapToMessageJob(item);
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_3mr15gia3().s[133]++;
      throw new Error(`Failed to generate message jobs: ${error instanceof Error ?
      /* istanbul ignore next */
      (cov_3mr15gia3().b[40][0]++, error.message) :
      /* istanbul ignore next */
      (cov_3mr15gia3().b[40][1]++, 'Unknown error')}`);
    }
  }
  /**
  * Get target contacts for campaign
  */
