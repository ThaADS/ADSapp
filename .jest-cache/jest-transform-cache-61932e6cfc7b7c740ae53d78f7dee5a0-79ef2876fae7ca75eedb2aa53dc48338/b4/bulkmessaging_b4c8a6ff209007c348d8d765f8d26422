  /**
  * Process message jobs for campaign
  */
  async processMessageJobs(campaignId) {
    /* istanbul ignore next */
    cov_3mr15gia3().f[20]++;
    cov_3mr15gia3().s[149]++;
    try {
      const campaign =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[150]++, await this.getCampaign(campaignId));
      /* istanbul ignore next */
      cov_3mr15gia3().s[151]++;
      if (campaign.status !== 'running') {
        /* istanbul ignore next */
        cov_3mr15gia3().b[49][0]++;
        cov_3mr15gia3().s[152]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_3mr15gia3().b[49][1]++;
      }
      // Get pending jobs scheduled for now or earlier
      const {
        data: jobs,
        error
      } =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[153]++, await this.supabase.from('bulk_message_jobs').select('*').eq('campaign_id', campaignId).eq('status', 'pending').lte('scheduled_at', new Date().toISOString()).order('scheduled_at', {
        ascending: true
      }).limit(100)) // Process in batches
      ;
      /* istanbul ignore next */
      cov_3mr15gia3().s[154]++;
      if (error) {
        /* istanbul ignore next */
        cov_3mr15gia3().b[50][0]++;
        cov_3mr15gia3().s[155]++;
        console.error('Failed to get message jobs:', error);
        /* istanbul ignore next */
        cov_3mr15gia3().s[156]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_3mr15gia3().b[50][1]++;
      }
      cov_3mr15gia3().s[157]++;
      if (
      /* istanbul ignore next */
      (cov_3mr15gia3().b[52][0]++, !jobs) ||
      /* istanbul ignore next */
      (cov_3mr15gia3().b[52][1]++, jobs.length === 0)) {
        /* istanbul ignore next */
        cov_3mr15gia3().b[51][0]++;
        cov_3mr15gia3().s[158]++;
        // Check if campaign is complete
        await this.checkCampaignCompletion(campaignId);
        /* istanbul ignore next */
        cov_3mr15gia3().s[159]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_3mr15gia3().b[51][1]++;
      }
      // Get WhatsApp client
      const config =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[160]++, await this.getWhatsAppConfig(campaign.organizationId));
      /* istanbul ignore next */
      cov_3mr15gia3().s[161]++;
      if (!config) {
        /* istanbul ignore next */
        cov_3mr15gia3().b[53][0]++;
        cov_3mr15gia3().s[162]++;
        throw new Error('WhatsApp not configured for organization');
      } else
      /* istanbul ignore next */
      {
        cov_3mr15gia3().b[53][1]++;
      }
      const whatsappClient =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[163]++, new _client.WhatsAppClient(config.accessToken, config.phoneNumberId));
      // Process jobs
      /* istanbul ignore next */
      cov_3mr15gia3().s[164]++;
      for (const jobData of jobs) {
        /* istanbul ignore next */
        cov_3mr15gia3().s[165]++;
        try {
          const job =
          /* istanbul ignore next */
          (cov_3mr15gia3().s[166]++, this.mapToMessageJob(jobData));
          /* istanbul ignore next */
          cov_3mr15gia3().s[167]++;
          await this.processMessageJob(job, campaign, whatsappClient);
        } catch (error) {
          /* istanbul ignore next */
          cov_3mr15gia3().s[168]++;
          console.error(`Failed to process job ${jobData.id}:`, error);
          // Update job with error
          /* istanbul ignore next */
          cov_3mr15gia3().s[169]++;
          await this.supabase.from('bulk_message_jobs').update({
            status: 'failed',
            error: error instanceof Error ?
            /* istanbul ignore next */
            (cov_3mr15gia3().b[54][0]++, error.message) :
            /* istanbul ignore next */
            (cov_3mr15gia3().b[54][1]++, 'Unknown error'),
            updated_at: new Date()
          }).eq('id', jobData.id);
        }
      }
      // Update campaign statistics
      /* istanbul ignore next */
      cov_3mr15gia3().s[170]++;
      await this.updateCampaignStatistics(campaignId);
      // Continue processing if there are more jobs
      /* istanbul ignore next */
      cov_3mr15gia3().s[171]++;
      setTimeout(() => {
        /* istanbul ignore next */
        cov_3mr15gia3().f[21]++;
        cov_3mr15gia3().s[172]++;
        return this.processMessageJobs(campaignId);
      }, 5000); // 5 second delay
    } catch (error) {
      /* istanbul ignore next */
      cov_3mr15gia3().s[173]++;
      console.error(`Failed to process message jobs for campaign ${campaignId}:`, error);
    }
  }
  /**
  * Process individual message job
  */
  async processMessageJob(job, campaign, whatsappClient) {
    /* istanbul ignore next */
    cov_3mr15gia3().f[22]++;
    cov_3mr15gia3().s[174]++;
    try {
      let messageResponse;
      // Send message based on campaign type
      /* istanbul ignore next */
      cov_3mr15gia3().s[175]++;
      if (campaign.templateId) {
        /* istanbul ignore next */
        cov_3mr15gia3().b[55][0]++;
        // Send template message
        const variables =
        /* istanbul ignore next */
        (cov_3mr15gia3().s[176]++,
        /* istanbul ignore next */
        (cov_3mr15gia3().b[56][0]++, campaign.message?.templateVariables) ||
        /* istanbul ignore next */
        (cov_3mr15gia3().b[56][1]++, {}));
        /* istanbul ignore next */
        cov_3mr15gia3().s[177]++;
        messageResponse = await this.templateManager.sendTemplateMessage(campaign.templateId, job.whatsappId, variables, whatsappClient.phoneNumberId, whatsappClient.accessToken);
      } else {
        /* istanbul ignore next */
        cov_3mr15gia3().b[55][1]++;
        cov_3mr15gia3().s[178]++;
        if (campaign.message) {
          /* istanbul ignore next */
          cov_3mr15gia3().b[57][0]++;
          cov_3mr15gia3().s[179]++;
          // Send regular message
          switch (campaign.message.type) {
            case 'text':
              /* istanbul ignore next */
              cov_3mr15gia3().b[58][0]++;
              cov_3mr15gia3().s[180]++;
              messageResponse = await whatsappClient.sendTextMessage(job.whatsappId, campaign.message.content);
              /* istanbul ignore next */
              cov_3mr15gia3().s[181]++;
              break;
            case 'media':
              /* istanbul ignore next */
              cov_3mr15gia3().b[58][1]++;
              cov_3mr15gia3().s[182]++;
              if (campaign.message.mediaUrl) {
                /* istanbul ignore next */
                cov_3mr15gia3().b[59][0]++;
                cov_3mr15gia3().s[183]++;
                messageResponse = await whatsappClient.sendImageMessage(job.whatsappId, campaign.message.mediaUrl, campaign.message.content);
              } else
              /* istanbul ignore next */
              {
                cov_3mr15gia3().b[59][1]++;
              }
              cov_3mr15gia3().s[184]++;
              break;
          }
        } else
        /* istanbul ignore next */
        {
          cov_3mr15gia3().b[57][1]++;
        }
      }
      // Update job as sent
      /* istanbul ignore next */
      cov_3mr15gia3().s[185]++;
      await this.supabase.from('bulk_message_jobs').update({
        status: 'sent',
        message_id: messageResponse?.messages?.[0]?.id,
        sent_at: new Date(),
        updated_at: new Date()
      }).eq('id', job.id);
    } catch (error) {
      /* istanbul ignore next */
      cov_3mr15gia3().s[186]++;
      // Handle retry logic
      if (job.retryCount < job.maxRetries) {
        /* istanbul ignore next */
        cov_3mr15gia3().b[60][0]++;
        cov_3mr15gia3().s[187]++;
        await this.supabase.from('bulk_message_jobs').update({
          retry_count: job.retryCount + 1,
          scheduled_at: new Date(Date.now() + Math.pow(2, job.retryCount) * 60000),
          updated_at: new Date()
        }).eq('id', job.id);
      } else {
        /* istanbul ignore next */
        cov_3mr15gia3().b[60][1]++;
        cov_3mr15gia3().s[188]++;
        await this.supabase.from('bulk_message_jobs').update({
          status: 'failed',
          error: error instanceof Error ?
          /* istanbul ignore next */
          (cov_3mr15gia3().b[61][0]++, error.message) :
          /* istanbul ignore next */
          (cov_3mr15gia3().b[61][1]++, 'Unknown error'),
          updated_at: new Date()
        }).eq('id', job.id);
      }
      /* istanbul ignore next */
      cov_3mr15gia3().s[189]++;
      throw error;
    }
  }
