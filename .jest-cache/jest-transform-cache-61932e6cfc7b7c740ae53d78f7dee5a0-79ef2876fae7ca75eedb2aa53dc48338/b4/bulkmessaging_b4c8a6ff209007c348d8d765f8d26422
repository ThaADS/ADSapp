    /* istanbul ignore next */
    (cov_3mr15gia3().s[134]++, this.supabase.from('contacts').select('id, whatsapp_id, name, tags').eq('organization_id', organizationId).eq('is_blocked', false));
    // Apply audience filters (same logic as calculateAudienceSize)
    /* istanbul ignore next */
    cov_3mr15gia3().s[135]++;
    switch (targetAudience.type) {
      case 'tags':
        /* istanbul ignore next */
        cov_3mr15gia3().b[41][0]++;
        cov_3mr15gia3().s[136]++;
        if (
        /* istanbul ignore next */
        (cov_3mr15gia3().b[43][0]++, targetAudience.tags) &&
        /* istanbul ignore next */
