class ContactListManager {
  /**
  * Create a new contact list
  */
  async createContactList(organizationId, list) {
    /* istanbul ignore next */
    cov_3mr15gia3().f[34]++;
    cov_3mr15gia3().s[237]++;
    try {
      const contactCount =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[238]++, await this.calculateListSize(organizationId, list.filters));
      const listData =
      /* istanbul ignore next */
      (cov_3mr15gia3().s[239]++, {
        id: _crypto.default.randomUUID(),
        organization_id: organizationId,
        name: list.name,
