    }
  }
  /**
  * Calculate list size based on filters
  */
  async calculateListSize(organizationId, filters) {
    /* istanbul ignore next */
    cov_3mr15gia3().f[35]++;
    let query =
    /* istanbul ignore next */
    (cov_3mr15gia3().s[245]++, this.supabase.from('contacts').select('id', {
      count: 'exact',
      head: true
    }).eq('organization_id', organizationId).eq('is_blocked', false));
    // Apply filters
    /* istanbul ignore next */
    cov_3mr15gia3().s[246]++;
    filters.forEach(filter => {
      /* istanbul ignore next */
      cov_3mr15gia3().f[36]++;
      cov_3mr15gia3().s[247]++;
      switch (filter.operator) {
        case 'equals':
          /* istanbul ignore next */
          cov_3mr15gia3().b[88][0]++;
          cov_3mr15gia3().s[248]++;
          query = query.eq(filter.field, filter.value);
          /* istanbul ignore next */
          cov_3mr15gia3().s[249]++;
          break;
        case 'contains':
          /* istanbul ignore next */
          cov_3mr15gia3().b[88][1]++;
          cov_3mr15gia3().s[250]++;
          query = query.like(filter.field, `%${filter.value}%`);
          /* istanbul ignore next */
          cov_3mr15gia3().s[251]++;
          break;
      }
    });
    const {
      count,
      error
    } =
    /* istanbul ignore next */
    (cov_3mr15gia3().s[252]++, await query);
    /* istanbul ignore next */
    cov_3mr15gia3().s[253]++;
    if (error) {
      /* istanbul ignore next */
      cov_3mr15gia3().b[89][0]++;
      cov_3mr15gia3().s[254]++;
      throw new Error(`Failed to calculate list size: ${error.message}`);
    } else
    /* istanbul ignore next */
    {
      cov_3mr15gia3().b[89][1]++;
    }
    cov_3mr15gia3().s[255]++;
    return /* istanbul ignore next */(cov_3mr15gia3().b[90][0]++, count) ||
    /* istanbul ignore next */
    (cov_3mr15gia3().b[90][1]++, 0);
  }
  /**
  * Map database record to contact list interface
  */
  mapToContactList(data) {
    /* istanbul ignore next */
    cov_3mr15gia3().f[37]++;
    cov_3mr15gia3().s[256]++;
    return {
      id: data.id,
      organizationId: data.organization_id,
      name: data.name,
      description: data.description,
      contactCount: data.contact_count,
      tags:
      /* istanbul ignore next */
      (cov_3mr15gia3().b[91][0]++, data.tags) ||
      /* istanbul ignore next */
      (cov_3mr15gia3().b[91][1]++, []),
      filters:
      /* istanbul ignore next */
      (cov_3mr15gia3().b[92][0]++, data.filters) ||
