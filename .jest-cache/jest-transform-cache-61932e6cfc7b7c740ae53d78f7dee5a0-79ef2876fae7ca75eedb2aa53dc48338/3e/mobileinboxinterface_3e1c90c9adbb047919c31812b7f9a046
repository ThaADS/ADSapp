    (cov_6abk3e6qx().b[50][0]++, showNewChatModal) &&
    /* istanbul ignore next */
    (cov_6abk3e6qx().b[50][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50 p-4",
      children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
        className: "bg-white rounded-lg shadow-xl w-full max-w-sm",
        children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "flex items-center justify-between p-4 border-b border-gray-200",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("h2", {
            className: "text-lg font-medium",
            children: "New Chat"
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
            onClick: () => {
              /* istanbul ignore next */
              cov_6abk3e6qx().f[47]++;
              cov_6abk3e6qx().s[138]++;
              return setShowNewChatModal(false);
            },
            className: "text-gray-400 hover:text-gray-600",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)(_outline.XMarkIcon, {
              className: "w-5 h-5"
            })
          })]
        }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "p-4",
          children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "space-y-3",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)("input", {
              type: "text",
              placeholder: "Enter phone number...",
              className: "w-full px-3 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-blue-500"
            }), /*#__PURE__*/(0, _jsxruntime.jsxs)("button", {
              className: "w-full flex items-center justify-center px-4 py-2 bg-blue-600 text-white rounded-lg hover:bg-blue-700",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_outline.UserPlusIcon, {
                className: "w-4 h-4 mr-2"
              }), "Start Chat"]
            })]
          })
        })]
      })
    }))]
  });
}
