      /* istanbul ignore next */
      (cov_6abk3e6qx().s[24]++, hasPropertyDescriptor ?
      /* istanbul ignore next */
      (cov_6abk3e6qx().b[11][0]++, Object.getOwnPropertyDescriptor(obj, key)) :
      /* istanbul ignore next */
      (cov_6abk3e6qx().b[11][1]++, null));
      /* istanbul ignore next */
      cov_6abk3e6qx().s[25]++;
