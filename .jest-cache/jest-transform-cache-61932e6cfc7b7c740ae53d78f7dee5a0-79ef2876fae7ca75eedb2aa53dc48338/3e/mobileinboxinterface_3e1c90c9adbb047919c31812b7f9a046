      } else {
        /* istanbul ignore next */
        cov_6abk3e6qx().b[12][1]++;
        cov_6abk3e6qx().s[27]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_6abk3e6qx().b[9][1]++;
    }
  }
  /* istanbul ignore next */
  cov_6abk3e6qx().s[28]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_6abk3e6qx().s[29]++;
  if (cache) {
    /* istanbul ignore next */
    cov_6abk3e6qx().b[14][0]++;
    cov_6abk3e6qx().s[30]++;
    cache.set(obj, newObj);
