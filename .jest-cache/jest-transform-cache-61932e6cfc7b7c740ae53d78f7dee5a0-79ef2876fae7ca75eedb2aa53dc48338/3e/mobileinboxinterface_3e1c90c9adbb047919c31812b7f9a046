  /* istanbul ignore next */
  (cov_6abk3e6qx().s[21]++,
  /* istanbul ignore next */
  (cov_6abk3e6qx().b[8][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_6abk3e6qx().b[8][1]++, Object.getOwnPropertyDescriptor));
  /* istanbul ignore next */
  cov_6abk3e6qx().s[22]++;
  for (var key in obj) {
    /* istanbul ignore next */
    cov_6abk3e6qx().s[23]++;
    if (
    /* istanbul ignore next */
    (cov_6abk3e6qx().b[10][0]++, key !== "default") &&
    /* istanbul ignore next */
    (cov_6abk3e6qx().b[10][1]++, Object.prototype.hasOwnProperty.call(obj, key))) {
      /* istanbul ignore next */
