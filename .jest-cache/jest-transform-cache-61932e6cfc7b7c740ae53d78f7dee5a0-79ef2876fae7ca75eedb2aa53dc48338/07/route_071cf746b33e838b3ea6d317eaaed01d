    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[41]++, {
      conversation_id: conversation.id,
      sender_type: body.sender_type,
      sender_id: body.sender_type === 'agent' ?
      /* istanbul ignore next */
      (cov_1tq2c1aacq().b[11][0]++, null) :
      /* istanbul ignore next */
      (cov_1tq2c1aacq().b[11][1]++, null),
      content: body.content,
      message_type: body.message_type,
      media_url: body.message_type !== 'text' ?
      /* istanbul ignore next */
      (cov_1tq2c1aacq().b[12][0]++, generateDemoMediaUrl(body.message_type)) :
      /* istanbul ignore next */
      (cov_1tq2c1aacq().b[12][1]++, null),
      media_mime_type: body.message_type !== 'text' ?
      /* istanbul ignore next */
      (cov_1tq2c1aacq().b[13][0]++, getDemoMimeType(body.message_type)) :
      /* istanbul ignore next */
      (cov_1tq2c1aacq().b[13][1]++, null),
      is_read: body.sender_type === 'agent',
      whatsapp_message_id: `demo_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`,
      created_at: new Date().toISOString()
    });
    const {
      data: message,
      error: messageError
    } =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[42]++, await supabase.from('messages').insert(messageData).select().single());
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[43]++;
    if (messageError) {
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[14][0]++;
      cov_1tq2c1aacq().s[44]++;
      console.error('Error creating demo message:', messageError);
      /* istanbul ignore next */
      cov_1tq2c1aacq().s[45]++;
      return _server.NextResponse.json({
        success: false,
        error: 'Failed to create message'
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_1tq2c1aacq().b[14][1]++;
    }
    // Update conversation last_message_at
    cov_1tq2c1aacq().s[46]++;
    await supabase.from('conversations').update({
      last_message_at: new Date().toISOString()
    }).eq('id', conversation.id);
    // Update contact last_message_at
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[47]++;
    await supabase.from('contacts').update({
      last_message_at: new Date().toISOString()
    }).eq('id', contact.id);
    // Trigger automation if requested
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[48]++;
    if (body.trigger_automation) {
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[15][0]++;
      cov_1tq2c1aacq().s[49]++;
      await triggerDemoAutomation(sessionResult.session, contact, message, body.content);
    } else
    /* istanbul ignore next */
    {
      cov_1tq2c1aacq().b[15][1]++;
    }
    // Track simulation event
    cov_1tq2c1aacq().s[50]++;
    await sessionManager.trackEvent(sessionResult.session.id, {
      event_type: 'feature_interaction',
      action: 'message_simulated',
      category: 'demo',
      label: `${body.sender_type}_${body.message_type}`,
      metadata: {
        contact_phone: body.contact_phone,
        message_length: body.content.length,
        has_delay: !!body.delay_seconds,
        trigger_automation: !!body.trigger_automation
      }
    });
    // Update session progress
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[51]++;
    await sessionManager.updateProgress(sessionToken, 'message_simulation', 'simulate_whatsapp_message', {
      message_type: body.message_type,
      sender_type: body.sender_type
    });
    const response =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[52]++, {
      success: true,
      data: {
        message_id: message.id,
        conversation_id: conversation.id,
        processed_at: new Date().toISOString()
      }
    });
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[53]++;
    return _server.NextResponse.json(response, {
      status: 201
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[54]++;
    console.error('Error simulating message:', error);
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[55]++;
    return _server.NextResponse.json({
      success: false,
      error: 'Failed to simulate message. Please try again.'
    }, {
      status: 500
    });
  }
}
async function GET(request) {
  /* istanbul ignore next */
  cov_1tq2c1aacq().f[7]++;
  cov_1tq2c1aacq().s[56]++;
  try {
    // Get session token from Authorization header
    const authHeader =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[57]++, request.headers.get('authorization'));
    const sessionToken =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[58]++, authHeader?.replace('Bearer ', ''));
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[59]++;
    if (!sessionToken) {
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[16][0]++;
      cov_1tq2c1aacq().s[60]++;
      return _server.NextResponse.json({
        success: false,
        error: 'Session token required in Authorization header'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_1tq2c1aacq().b[16][1]++;
    }
    // Initialize Supabase client
    const supabaseUrl =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[61]++, process.env.NEXT_PUBLIC_SUPABASE_URL);
    const supabaseServiceKey =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[62]++, process.env.SUPABASE_SERVICE_ROLE_KEY);
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[63]++;
    if (
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[18][0]++, !supabaseUrl) ||
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[18][1]++, !supabaseServiceKey)) {
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[17][0]++;
      cov_1tq2c1aacq().s[64]++;
      return _server.NextResponse.json({
        success: false,
        error: 'Service configuration error'
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_1tq2c1aacq().b[17][1]++;
    }
    const supabase =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[65]++, (0, _supabasejs.createClient)(supabaseUrl, supabaseServiceKey));
    // Validate session
    const sessionManager =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[66]++, new _demo.DemoSessionManager(supabase));
    const sessionResult =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[67]++, await sessionManager.getSession(sessionToken));
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[68]++;
    if (
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[20][0]++, !sessionResult.session) ||
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[20][1]++, !sessionResult.isValid)) {
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[19][0]++;
      cov_1tq2c1aacq().s[69]++;
      return _server.NextResponse.json({
        success: false,
        error: 'Invalid or expired session'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_1tq2c1aacq().b[19][1]++;
    }
    // Get available contacts for this demo session
    const {
      data: contacts
    } =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[70]++, await supabase.from('contacts').select('phone_number, name, tags').eq('organization_id', sessionResult.session.organization_id).order('created_at', {
      ascending: true
    }));
    // Get scenario-specific message templates
    const messageTemplates =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[71]++, getScenarioMessageTemplates(sessionResult.session.business_scenario));
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[72]++;
    return _server.NextResponse.json({
      success: true,
      data: {
        session_info: {
          scenario: sessionResult.session.business_scenario,
          organization_id: sessionResult.session.organization_id
        },
        available_contacts:
        /* istanbul ignore next */
        (cov_1tq2c1aacq().b[21][0]++, contacts) ||
        /* istanbul ignore next */
        (cov_1tq2c1aacq().b[21][1]++, []),
        message_types: ['text', 'image', 'document'],
        sender_types: ['contact', 'agent'],
        max_delay_seconds: 30,
        message_templates: messageTemplates,
        automation_available: true
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[73]++;
    console.error('Error getting simulation options:', error);
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[74]++;
    return _server.NextResponse.json({
      success: false,
      error: 'Failed to get simulation options'
    }, {
      status: 500
    });
  }
}
async function PUT(request) {
  /* istanbul ignore next */
  cov_1tq2c1aacq().f[8]++;
  cov_1tq2c1aacq().s[75]++;
  try {
    // Get session token from Authorization header
    const authHeader =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[76]++, request.headers.get('authorization'));
    const sessionToken =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[77]++, authHeader?.replace('Bearer ', ''));
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[78]++;
    if (!sessionToken) {
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[22][0]++;
      cov_1tq2c1aacq().s[79]++;
      return _server.NextResponse.json({
        success: false,
        error: 'Session token required in Authorization header'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_1tq2c1aacq().b[22][1]++;
    }
    // Parse request body
    const body =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[80]++, await request.json());
    const {
      messages,
      scenario_name
    } =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[81]++, body);
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[82]++;
    if (
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[24][0]++, !messages) ||
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[24][1]++, !Array.isArray(messages)) ||
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[24][2]++, messages.length === 0)) {
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[23][0]++;
      cov_1tq2c1aacq().s[83]++;
      return _server.NextResponse.json({
        success: false,
        error: 'messages array is required and must not be empty'
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_1tq2c1aacq().b[23][1]++;
    }
    cov_1tq2c1aacq().s[84]++;
    if (messages.length > 20) {
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[25][0]++;
      cov_1tq2c1aacq().s[85]++;
      return _server.NextResponse.json({
        success: false,
        error: 'Maximum 20 messages allowed per batch'
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_1tq2c1aacq().b[25][1]++;
    }
    // Initialize Supabase client
    const supabaseUrl =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[86]++, process.env.NEXT_PUBLIC_SUPABASE_URL);
    const supabaseServiceKey =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[87]++, process.env.SUPABASE_SERVICE_ROLE_KEY);
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[88]++;
    if (
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[27][0]++, !supabaseUrl) ||
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[27][1]++, !supabaseServiceKey)) {
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[26][0]++;
      cov_1tq2c1aacq().s[89]++;
      return _server.NextResponse.json({
        success: false,
        error: 'Service configuration error'
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_1tq2c1aacq().b[26][1]++;
    }
    const supabase =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[90]++, (0, _supabasejs.createClient)(supabaseUrl, supabaseServiceKey));
    // Validate session
    const sessionManager =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[91]++, new _demo.DemoSessionManager(supabase));
    const sessionResult =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[92]++, await sessionManager.getSession(sessionToken));
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[93]++;
    if (
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[29][0]++, !sessionResult.session) ||
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[29][1]++, !sessionResult.isValid)) {
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[28][0]++;
      cov_1tq2c1aacq().s[94]++;
      return _server.NextResponse.json({
        success: false,
        error: 'Invalid or expired session'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_1tq2c1aacq().b[28][1]++;
    }
    const results =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[95]++, []);
    const errors =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[96]++, []);
    // Process messages with delays between them
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[97]++;
    for (let i =
    /* istanbul ignore next */
    (cov_1tq2c1aacq().s[98]++, 0); i < messages.length; i++) {
      const messageData =
      /* istanbul ignore next */
      (cov_1tq2c1aacq().s[99]++, messages[i]);
      /* istanbul ignore next */
      cov_1tq2c1aacq().s[100]++;
      try {
        /* istanbul ignore next */
        cov_1tq2c1aacq().s[101]++;
        // Validate message data
        if (
        /* istanbul ignore next */
        (cov_1tq2c1aacq().b[31][0]++, !messageData.contact_phone) ||
        /* istanbul ignore next */
        (cov_1tq2c1aacq().b[31][1]++, !messageData.content) ||
        /* istanbul ignore next */
        (cov_1tq2c1aacq().b[31][2]++, !messageData.message_type)) {
          /* istanbul ignore next */
          cov_1tq2c1aacq().b[30][0]++;
          cov_1tq2c1aacq().s[102]++;
          errors.push({
            index: i,
            error: 'Missing required fields: contact_phone, content, message_type'
          });
          /* istanbul ignore next */
          cov_1tq2c1aacq().s[103]++;
          continue;
        } else
        /* istanbul ignore next */
        {
          cov_1tq2c1aacq().b[30][1]++;
        }
        // Simulate message by calling the main logic
        const result =
        /* istanbul ignore next */
        (cov_1tq2c1aacq().s[104]++, await simulateSingleMessage(supabase, sessionResult.session.organization_id, messageData, sessionResult.session.business_scenario));
        /* istanbul ignore next */
        cov_1tq2c1aacq().s[105]++;
        results.push({
          index: i,
          message_id: result.message_id,
          conversation_id: result.conversation_id
        });
        // Add delay between messages to make it more realistic
        /* istanbul ignore next */
        cov_1tq2c1aacq().s[106]++;
        if (i < messages.length - 1) {
          /* istanbul ignore next */
          cov_1tq2c1aacq().b[32][0]++;
          cov_1tq2c1aacq().s[107]++;
          await new Promise(resolve => {
            /* istanbul ignore next */
            cov_1tq2c1aacq().f[9]++;
            cov_1tq2c1aacq().s[108]++;
            return setTimeout(resolve, 1000 + Math.random() * 2000);
          });
        } else
        /* istanbul ignore next */
        {
          cov_1tq2c1aacq().b[32][1]++;
        }
      } catch (error) {
        /* istanbul ignore next */
        cov_1tq2c1aacq().s[109]++;
        console.error(`Error processing message ${i}:`, error);
        /* istanbul ignore next */
        cov_1tq2c1aacq().s[110]++;
        errors.push({
          index: i,
          error: error instanceof Error ?
          /* istanbul ignore next */
          (cov_1tq2c1aacq().b[33][0]++, error.message) :
          /* istanbul ignore next */
          (cov_1tq2c1aacq().b[33][1]++, 'Unknown error')
        });
      }
    }
    // Track batch simulation event
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[111]++;
    await sessionManager.trackEvent(sessionResult.session.id, {
      event_type: 'feature_interaction',
      action: 'batch_messages_simulated',
      category: 'demo',
      label:
      /* istanbul ignore next */
      (cov_1tq2c1aacq().b[34][0]++, scenario_name) ||
      /* istanbul ignore next */
      (cov_1tq2c1aacq().b[34][1]++, 'custom_batch'),
      value: results.length,
      metadata: {
        total_messages: messages.length,
        successful: results.length,
        failed: errors.length
      }
    });
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[112]++;
    return _server.NextResponse.json({
      success: true,
      data: {
        processed_at: new Date().toISOString(),
        total_messages: messages.length,
        successful: results.length,
        failed: errors.length,
        results: results,
        errors: errors.length > 0 ?
        /* istanbul ignore next */
        (cov_1tq2c1aacq().b[35][0]++, errors) :
        /* istanbul ignore next */
        (cov_1tq2c1aacq().b[35][1]++, undefined)
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[113]++;
    console.error('Error in batch message simulation:', error);
    /* istanbul ignore next */
    cov_1tq2c1aacq().s[114]++;
    return _server.NextResponse.json({
      success: false,
      error: 'Failed to process batch messages'
    }, {
      status: 500
    });
  }
}
// Helper functions
async function findOrCreateContact(supabase, organizationId, phoneNumber, scenario) {
  /* istanbul ignore next */
  cov_1tq2c1aacq().f[10]++;
  // Try to find existing contact
  const {
    data: existingContact
  } =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[115]++, await supabase.from('contacts').select('*').eq('organization_id', organizationId).eq('phone_number', phoneNumber).single());
  /* istanbul ignore next */
  cov_1tq2c1aacq().s[116]++;
  if (existingContact) {
    /* istanbul ignore next */
    cov_1tq2c1aacq().b[36][0]++;
    cov_1tq2c1aacq().s[117]++;
    return existingContact;
  } else
  /* istanbul ignore next */
  {
    cov_1tq2c1aacq().b[36][1]++;
  }
  // Create new contact
  const contactData =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[118]++, {
    organization_id: organizationId,
    whatsapp_id: `demo_${phoneNumber.replace('+', '')}_${Date.now()}`,
    phone_number: phoneNumber,
    name: generateDemoContactName(phoneNumber, scenario),
    tags: getScenarioTags(scenario),
    notes: `Demo contact for ${scenario} scenario`,
    is_blocked: false,
    created_at: new Date().toISOString(),
    updated_at: new Date().toISOString()
  });
  const {
    data: newContact,
    error
  } =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[119]++, await supabase.from('contacts').insert(contactData).select().single());
  /* istanbul ignore next */
  cov_1tq2c1aacq().s[120]++;
  if (error) {
    /* istanbul ignore next */
    cov_1tq2c1aacq().b[37][0]++;
    cov_1tq2c1aacq().s[121]++;
    throw new Error(`Failed to create contact: ${error.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_1tq2c1aacq().b[37][1]++;
  }
  cov_1tq2c1aacq().s[122]++;
  return newContact;
}
async function findOrCreateConversation(supabase, organizationId, contactId) {
  /* istanbul ignore next */
  cov_1tq2c1aacq().f[11]++;
  // Try to find existing conversation
  const {
    data: existingConversation
  } =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[123]++, await supabase.from('conversations').select('*').eq('organization_id', organizationId).eq('contact_id', contactId).eq('status', 'open').single());
  /* istanbul ignore next */
  cov_1tq2c1aacq().s[124]++;
  if (existingConversation) {
    /* istanbul ignore next */
    cov_1tq2c1aacq().b[38][0]++;
    cov_1tq2c1aacq().s[125]++;
    return existingConversation;
  } else
  /* istanbul ignore next */
  {
    cov_1tq2c1aacq().b[38][1]++;
  }
  // Create new conversation
  const conversationData =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[126]++, {
    organization_id: organizationId,
    contact_id: contactId,
    status: 'open',
    priority: 'medium',
    subject: 'Demo conversation',
    created_at: new Date().toISOString(),
    updated_at: new Date().toISOString()
  });
  const {
    data: newConversation,
    error
  } =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[127]++, await supabase.from('conversations').insert(conversationData).select().single());
  /* istanbul ignore next */
  cov_1tq2c1aacq().s[128]++;
  if (error) {
    /* istanbul ignore next */
    cov_1tq2c1aacq().b[39][0]++;
    cov_1tq2c1aacq().s[129]++;
    throw new Error(`Failed to create conversation: ${error.message}`);
  } else
  /* istanbul ignore next */
  {
    cov_1tq2c1aacq().b[39][1]++;
  }
  cov_1tq2c1aacq().s[130]++;
  return newConversation;
}
async function simulateSingleMessage(supabase, organizationId, messageData, scenario) {
  /* istanbul ignore next */
  cov_1tq2c1aacq().f[12]++;
  const contact =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[131]++, await findOrCreateContact(supabase, organizationId, messageData.contact_phone, scenario));
  const conversation =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[132]++, await findOrCreateConversation(supabase, organizationId, contact.id));
  const message =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[133]++, {
    conversation_id: conversation.id,
    sender_type:
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[40][0]++, messageData.sender_type) ||
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[40][1]++, 'contact'),
    content: messageData.content,
    message_type:
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[41][0]++, messageData.message_type) ||
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[41][1]++, 'text'),
    media_url: messageData.message_type !== 'text' ?
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[42][0]++, generateDemoMediaUrl(messageData.message_type)) :
    /* istanbul ignore next */
    (cov_1tq2c1aacq().b[42][1]++, null),
    is_read: messageData.sender_type === 'agent',
    whatsapp_message_id: `demo_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`,
    created_at: new Date().toISOString()
  });
  const {
    data: newMessage,
    error
  } =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[134]++, await supabase.from('messages').insert(message).select().single());
  /* istanbul ignore next */
  cov_1tq2c1aacq().s[135]++;
  if (error) {
    /* istanbul ignore next */
    cov_1tq2c1aacq().b[43][0]++;
    cov_1tq2c1aacq().s[136]++;
    throw error;
  } else
  /* istanbul ignore next */
  {
    cov_1tq2c1aacq().b[43][1]++;
  }
  // Update timestamps
  cov_1tq2c1aacq().s[137]++;
  await supabase.from('conversations').update({
    last_message_at: new Date().toISOString()
  }).eq('id', conversation.id);
  /* istanbul ignore next */
  cov_1tq2c1aacq().s[138]++;
  await supabase.from('contacts').update({
    last_message_at: new Date().toISOString()
  }).eq('id', contact.id);
  /* istanbul ignore next */
  cov_1tq2c1aacq().s[139]++;
  return {
    message_id: newMessage.id,
    conversation_id: conversation.id
  };
}
function generateDemoContactName(phoneNumber, scenario) {
  /* istanbul ignore next */
  cov_1tq2c1aacq().f[13]++;
  const names =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[140]++, {
    retail: ['Fashion Lover', 'Style Seeker', 'Trendy Customer'],
    restaurant: ['Food Enthusiast', 'Regular Diner', 'Hungry Customer'],
    real_estate: ['Home Buyer', 'Property Investor', 'House Hunter'],
    healthcare: ['Patient', 'Health Conscious', 'Wellness Seeker'],
    education: ['Student', 'Learner', 'Knowledge Seeker'],
    ecommerce: ['Online Shopper', 'Tech Buyer', 'Deal Hunter'],
    automotive: ['Car Buyer', 'Auto Enthusiast', 'Driver'],
    travel: ['Traveler', 'Adventure Seeker', 'Tourist'],
    fitness: ['Fitness Enthusiast', 'Gym Member', 'Health Conscious'],
    generic: ['Demo User', 'Customer', 'Client']
  });
  const scenarioNames =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[141]++,
  /* istanbul ignore next */
  (cov_1tq2c1aacq().b[44][0]++, names[scenario]) ||
  /* istanbul ignore next */
  (cov_1tq2c1aacq().b[44][1]++, names.generic));
  const hash =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[142]++, phoneNumber.split('').reduce((a, b) => {
    /* istanbul ignore next */
    cov_1tq2c1aacq().f[14]++;
    cov_1tq2c1aacq().s[143]++;
    return a + b.charCodeAt(0);
  }, 0));
  /* istanbul ignore next */
  cov_1tq2c1aacq().s[144]++;
  return scenarioNames[hash % scenarioNames.length];
}
