function generateDemoMediaUrl(messageType) {
  /* istanbul ignore next */
  cov_1tq2c1aacq().f[16]++;
  const baseUrl =
  /* istanbul ignore next */
  (cov_1tq2c1aacq().s[147]++, 'https://picsum.photos');
  /* istanbul ignore next */
  cov_1tq2c1aacq().s[148]++;
  switch (messageType) {
    case 'image':
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[46][0]++;
      cov_1tq2c1aacq().s[149]++;
      return `${baseUrl}/400/300?random=${Math.random()}`;
    case 'document':
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[46][1]++;
      cov_1tq2c1aacq().s[150]++;
      return `https://www.w3.org/WAI/ER/tests/xhtml/testfiles/resources/pdf/dummy.pdf`;
    default:
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[46][2]++;
      cov_1tq2c1aacq().s[151]++;
      return '';
  }
}
function getDemoMimeType(messageType) {
  /* istanbul ignore next */
  cov_1tq2c1aacq().f[17]++;
  cov_1tq2c1aacq().s[152]++;
  switch (messageType) {
    case 'image':
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[47][0]++;
      cov_1tq2c1aacq().s[153]++;
      return 'image/jpeg';
    case 'document':
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[47][1]++;
      cov_1tq2c1aacq().s[154]++;
      return 'application/pdf';
    default:
      /* istanbul ignore next */
      cov_1tq2c1aacq().b[47][2]++;
      cov_1tq2c1aacq().s[155]++;
      return 'text/plain';
  }
}
