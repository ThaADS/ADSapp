                                        placeholder: `Message ${contactName}...`,
                                        className: "w-full px-4 py-3 pr-12 border border-gray-300 rounded-lg resize-none focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-transparent",
                                        rows: 1,
                                        style: {
                                            minHeight: '44px',
                                            maxHeight: '120px',
                                            resize: 'none'
                                        },
                                        onInput: (e)=>{
                                            const target = e.target;
                                            target.style.height = 'auto';
                                            target.style.height = `${Math.min(target.scrollHeight, 120)}px`;
                                        }
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "absolute right-3 bottom-3 flex items-center space-x-1",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                                onClick: ()=>setShowFormatting(!showFormatting),
                                                className: `p-1 rounded transition-colors ${showFormatting ? 'text-blue-600' : 'text-gray-400 hover:text-gray-600'}`,
                                                title: "Text formatting",
                                                children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.BoldIcon, {
                                                    className: "w-4 h-4"
                                                })
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                                onClick: ()=>setShowEmojiPicker(!showEmojiPicker),
                                                className: "p-1 text-gray-400 hover:text-gray-600 rounded transition-colors",
                                                title: "Add emoji",
                                                children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.FaceSmileIcon, {
                                                    className: "w-4 h-4"
                                                })
                                            }),
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                                onClick: ()=>setShowTemplates(!showTemplates),
                                                className: "p-1 text-gray-400 hover:text-gray-600 rounded transition-colors",
                                                title: "Use template",
                                                children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.AtSymbolIcon, {
                                                    className: "w-4 h-4"
                                                })
                                            })
                                        ]
                                    })
                                ]
                            }),
                            messageInput.trim() || attachments.length > 0 ? /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                onClick: sendMessage,
                                className: "p-2 bg-blue-600 text-white rounded-lg hover:bg-blue-700 transition-colors",
                                title: "Send message",
                                children: /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.PaperAirplaneIcon, {
                                    className: "w-5 h-5"
                                })
                            }) : /*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                onMouseDown: startRecording,
                                onMouseUp: stopRecording,
                                onMouseLeave: stopRecording,
                                className: `p-2 rounded-lg transition-colors ${isRecording ? 'bg-red-600 text-white' : 'bg-gray-100 text-gray-600 hover:bg-gray-200'}`,
                                title: isRecording ? 'Release to send' : 'Hold to record',
                                children: isRecording ? /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                    className: "flex items-center space-x-1",
                                    children: [
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                            className: "w-2 h-2 bg-white rounded-full animate-pulse"
                                        }),
                                        /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                            className: "text-xs",
                                            children: formatRecordingTime(recordingTime)
                                        })
                                    ]
                                }) : /*#__PURE__*/ (0, _jsxruntime.jsx)(_outline.MicrophoneIcon, {
                                    className: "w-5 h-5"
                                })
                            })
                        ]
                    })
                ]
            }),
            showEmojiPicker && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                className: "absolute bottom-20 right-4 bg-white border border-gray-200 rounded-lg shadow-lg p-4 z-50 w-80",
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "flex space-x-2 mb-3",
                        children: Object.keys(EMOJI_CATEGORIES).map((category)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                className: "px-2 py-1 text-xs bg-gray-100 hover:bg-gray-200 rounded capitalize",
                                children: category
                            }, category))
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "grid grid-cols-8 gap-1 max-h-48 overflow-y-auto",
                        children: EMOJI_CATEGORIES.recent.concat(EMOJI_CATEGORIES.people, EMOJI_CATEGORIES.nature, EMOJI_CATEGORIES.objects, EMOJI_CATEGORIES.symbols).map((emoji, index)=>/*#__PURE__*/ (0, _jsxruntime.jsx)("button", {
                                onClick: ()=>insertEmoji(emoji),
                                className: "p-2 hover:bg-gray-100 rounded text-lg",
                                children: emoji
                            }, index))
                    })
                ]
            }),
            showTemplates && /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                className: "absolute bottom-20 left-4 bg-white border border-gray-200 rounded-lg shadow-lg p-4 z-50 w-96",
                children: [
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("h3", {
                        className: "font-medium text-gray-900 mb-3",
                        children: "Message Templates"
                    }),
                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                        className: "space-y-2 max-h-64 overflow-y-auto",
                        children: SAMPLE_TEMPLATES.map((template)=>/*#__PURE__*/ (0, _jsxruntime.jsxs)("button", {
                                onClick: ()=>useTemplate(template),
                                className: "w-full text-left p-3 border border-gray-200 rounded-lg hover:border-blue-500 hover:bg-blue-50 transition-colors",
                                children: [
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                        className: "font-medium text-sm text-gray-900",
                                        children: template.name
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsx)("div", {
                                        className: "text-xs text-gray-600 mt-1 line-clamp-2",
                                        children: template.content
                                    }),
                                    /*#__PURE__*/ (0, _jsxruntime.jsxs)("div", {
                                        className: "flex items-center mt-2",
                                        children: [
                                            /*#__PURE__*/ (0, _jsxruntime.jsx)("span", {
                                                className: "text-xs bg-gray-100 text-gray-600 px-2 py-1 rounded-full",
                                                children: template.category
                                            }),
                                            template.variables.length > 0 && /*#__PURE__*/ (0, _jsxruntime.jsxs)("span", {
                                                className: "text-xs text-gray-500 ml-2",
                                                children: [
                                                    template.variables.length,
                                                    " variables"
                                                ]
                                            })
                                        ]
                                    })
                                ]
                            }, template.id))
                    })
                ]
            }),
            /*#__PURE__*/ (0, _jsxruntime.jsx)("input", {
                ref: fileInputRef,
                type: "file",
                multiple: true,
                accept: "image/*,video/*,audio/*,.pdf,.doc,.docx,.txt",
                onChange: (e)=>handleFileSelect(e.target.files),
                className: "hidden"
            })
        ]
    });
}

