        /* istanbul ignore next */
        cov_2kr9tdcgh8().s[50]++;
        break;
      case 'bulk_conversation_close':
        /* istanbul ignore next */
        cov_2kr9tdcgh8().b[2][3]++;
        // Estimate conversations to close
        const convCount =
        /* istanbul ignore next */
        (cov_2kr9tdcgh8().s[51]++, await estimateConversationsToClose(profile.organization_id, configuration));
        /* istanbul ignore next */
        cov_2kr9tdcgh8().s[52]++;
        totalItems = convCount;
        /* istanbul ignore next */
        cov_2kr9tdcgh8().s[53]++;
        break;
      default:
        /* istanbul ignore next */
        cov_2kr9tdcgh8().b[2][4]++;
        cov_2kr9tdcgh8().s[54]++;
        return _server.NextResponse.json({
          error: 'Invalid operation type'
        }, {
          status: 400
        });
    }
    // Create the bulk operation
    const queue =
    /* istanbul ignore next */
    (cov_2kr9tdcgh8().s[55]++, new _queue.BulkOperationQueue());
    const operation =
    /* istanbul ignore next */
    (cov_2kr9tdcgh8().s[56]++, await queue.createOperation({
      organizationId: profile.organization_id,
      userId: user.id,
      type,
      status: 'queued',
      totalItems,
      configuration
    }));
    // Queue the operation for processing
    /* istanbul ignore next */
    cov_2kr9tdcgh8().s[57]++;
    await queueOperationForProcessing(operation.id);
    /* istanbul ignore next */
    cov_2kr9tdcgh8().s[58]++;
    return (0, _apiutils.createSuccessResponse)(operation, 201);
  } catch (error) {
    /* istanbul ignore next */
    cov_2kr9tdcgh8().s[59]++;
    console.error('Create bulk operation error:', error);
    /* istanbul ignore next */
    cov_2kr9tdcgh8().s[60]++;
    return (0, _apiutils.createErrorResponse)(error);
  }
}
async function checkMessageLimit(organizationId, messageCount) {
  /* istanbul ignore next */
  cov_2kr9tdcgh8().f[6]++;
  const supabase =
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().s[61]++, await (0, _server1.createClient)());
  // Get organization subscription details
  const {
    data: org
  } =
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().s[62]++, await supabase.from('organizations').select('subscription_tier, subscription_status').eq('id', organizationId).single());
  /* istanbul ignore next */
  cov_2kr9tdcgh8().s[63]++;
  if (!org) {
    /* istanbul ignore next */
    cov_2kr9tdcgh8().b[11][0]++;
    cov_2kr9tdcgh8().s[64]++;
    return {
      allowed: false,
      reason: 'Organization not found'
    };
  } else
  /* istanbul ignore next */
  {
    cov_2kr9tdcgh8().b[11][1]++;
  }
  // Check subscription status
  cov_2kr9tdcgh8().s[65]++;
  if (
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().b[13][0]++, org.subscription_status !== 'active') &&
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().b[13][1]++, org.subscription_status !== 'trial')) {
    /* istanbul ignore next */
    cov_2kr9tdcgh8().b[12][0]++;
    cov_2kr9tdcgh8().s[66]++;
    return {
      allowed: false,
      reason: 'Active subscription required for bulk messaging'
    };
  } else
  /* istanbul ignore next */
  {
    cov_2kr9tdcgh8().b[12][1]++;
  }
  // Check tier limits
  const limits =
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().s[67]++, {
    starter: 100,
    professional: 1000,
    enterprise: 10000
  });
  const limit =
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().s[68]++,
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().b[14][0]++, limits[org.subscription_tier]) ||
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().b[14][1]++, 0));
  /* istanbul ignore next */
  cov_2kr9tdcgh8().s[69]++;
  if (messageCount > limit) {
    /* istanbul ignore next */
    cov_2kr9tdcgh8().b[15][0]++;
    cov_2kr9tdcgh8().s[70]++;
    return {
      allowed: false,
      reason: `Bulk message limit exceeded. Your ${org.subscription_tier} plan allows up to ${limit} messages per operation.`
    };
  } else
  /* istanbul ignore next */
  {
    cov_2kr9tdcgh8().b[15][1]++;
  }
  // Check daily usage (optional additional check)
  const today =
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().s[71]++, new Date().toISOString().split('T')[0]);
  const {
    data: dailyUsage
  } =
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().s[72]++, await supabase.from('daily_analytics').select('messages_sent').eq('organization_id', organizationId).eq('date', today).single());
  const messagesUsedToday =
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().s[73]++,
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().b[16][0]++, dailyUsage?.messages_sent) ||
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().b[16][1]++, 0));
  const dailyLimit =
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().s[74]++, limits[org.subscription_tier] * 10) // 10x the per-operation limit as daily limit
  ;
  /* istanbul ignore next */
  cov_2kr9tdcgh8().s[75]++;
  if (messagesUsedToday + messageCount > dailyLimit) {
    /* istanbul ignore next */
    cov_2kr9tdcgh8().b[17][0]++;
    cov_2kr9tdcgh8().s[76]++;
    return {
      allowed: false,
      reason: `Daily message limit would be exceeded. Used: ${messagesUsedToday}, Attempting: ${messageCount}, Daily limit: ${dailyLimit}`
    };
  } else
  /* istanbul ignore next */
  {
    cov_2kr9tdcgh8().b[17][1]++;
  }
  cov_2kr9tdcgh8().s[77]++;
  return {
    allowed: true
  };
}
async function estimateFileSize(fileUrl, format) {
  /* istanbul ignore next */
  cov_2kr9tdcgh8().f[7]++;
  cov_2kr9tdcgh8().s[78]++;
  try {
    // Make a HEAD request to get file size
    const response =
    /* istanbul ignore next */
    (cov_2kr9tdcgh8().s[79]++, await fetch(fileUrl, {
      method: 'HEAD'
    }));
    const contentLength =
    /* istanbul ignore next */
    (cov_2kr9tdcgh8().s[80]++, response.headers.get('content-length'));
    /* istanbul ignore next */
    cov_2kr9tdcgh8().s[81]++;
    if (contentLength) {
      /* istanbul ignore next */
      cov_2kr9tdcgh8().b[18][0]++;
      const fileSizeBytes =
      /* istanbul ignore next */
      (cov_2kr9tdcgh8().s[82]++, parseInt(contentLength));
      // Rough estimates based on file format
      let estimatedRows;
      /* istanbul ignore next */
      cov_2kr9tdcgh8().s[83]++;
      switch (format) {
        case 'csv':
          /* istanbul ignore next */
          cov_2kr9tdcgh8().b[19][0]++;
          cov_2kr9tdcgh8().s[84]++;
          estimatedRows = Math.floor(fileSizeBytes / 100); // ~100 bytes per row
          /* istanbul ignore next */
          cov_2kr9tdcgh8().s[85]++;
          break;
        case 'xlsx':
          /* istanbul ignore next */
          cov_2kr9tdcgh8().b[19][1]++;
          cov_2kr9tdcgh8().s[86]++;
          estimatedRows = Math.floor(fileSizeBytes / 200); // ~200 bytes per row
          /* istanbul ignore next */
          cov_2kr9tdcgh8().s[87]++;
          break;
        case 'json':
          /* istanbul ignore next */
          cov_2kr9tdcgh8().b[19][2]++;
          cov_2kr9tdcgh8().s[88]++;
          estimatedRows = Math.floor(fileSizeBytes / 300); // ~300 bytes per row
          /* istanbul ignore next */
          cov_2kr9tdcgh8().s[89]++;
          break;
        default:
          /* istanbul ignore next */
          cov_2kr9tdcgh8().b[19][3]++;
          cov_2kr9tdcgh8().s[90]++;
          estimatedRows = Math.floor(fileSizeBytes / 150);
      }
      /* istanbul ignore next */
      cov_2kr9tdcgh8().s[91]++;
      return Math.max(1, estimatedRows);
    } else
    /* istanbul ignore next */
    {
      cov_2kr9tdcgh8().b[18][1]++;
    }
    cov_2kr9tdcgh8().s[92]++;
    return 1000 // Default estimate if size can't be determined
    ;
  } catch (error) {
    /* istanbul ignore next */
    cov_2kr9tdcgh8().s[93]++;
    console.error('Error estimating file size:', error);
    /* istanbul ignore next */
    cov_2kr9tdcgh8().s[94]++;
    return 1000 // Default estimate
    ;
  }
}
async function estimateConversationsToClose(organizationId, config) {
  /* istanbul ignore next */
  cov_2kr9tdcgh8().f[8]++;
  const supabase =
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().s[95]++, await (0, _server1.createClient)());
  let query =
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().s[96]++, supabase.from('conversations').select('id', {
    count: 'exact'
  }).eq('organization_id', organizationId));
  /* istanbul ignore next */
  cov_2kr9tdcgh8().s[97]++;
  if (config.status) {
    /* istanbul ignore next */
    cov_2kr9tdcgh8().b[20][0]++;
    cov_2kr9tdcgh8().s[98]++;
    query = query.eq('status', config.status);
  } else
  /* istanbul ignore next */
  {
    cov_2kr9tdcgh8().b[20][1]++;
  }
  cov_2kr9tdcgh8().s[99]++;
  if (config.olderThanDays) {
    /* istanbul ignore next */
    cov_2kr9tdcgh8().b[21][0]++;
    const cutoffDate =
    /* istanbul ignore next */
    (cov_2kr9tdcgh8().s[100]++, new Date(Date.now() - config.olderThanDays * 24 * 60 * 60 * 1000).toISOString());
    /* istanbul ignore next */
    cov_2kr9tdcgh8().s[101]++;
    query = query.lt('last_message_at', cutoffDate);
  } else
  /* istanbul ignore next */
  {
    cov_2kr9tdcgh8().b[21][1]++;
  }
  const {
    count
  } =
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().s[102]++, await query);
  /* istanbul ignore next */
  cov_2kr9tdcgh8().s[103]++;
  return /* istanbul ignore next */(cov_2kr9tdcgh8().b[22][0]++, count) ||
  /* istanbul ignore next */
  (cov_2kr9tdcgh8().b[22][1]++, 0);
}
