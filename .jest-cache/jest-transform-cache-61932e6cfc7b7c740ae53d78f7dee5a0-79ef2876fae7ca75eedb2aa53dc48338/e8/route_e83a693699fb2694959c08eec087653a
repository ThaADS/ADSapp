    (cov_5mnui7zlw().b[3][1]++, []));
    /* istanbul ignore next */
    cov_5mnui7zlw().s[21]++;
    validationResults.variableValidation.detected = detectedVariables;
    // Check for missing variable declarations
    /* istanbul ignore next */
    cov_5mnui7zlw().s[22]++;
    for (const detectedVar of detectedVariables) {
      const isDeclared =
      /* istanbul ignore next */
      (cov_5mnui7zlw().s[23]++, declaredVariables.some(declared => {
        /* istanbul ignore next */
        cov_5mnui7zlw().f[2]++;
        cov_5mnui7zlw().s[24]++;
        return declared.name === detectedVar.name;
      }));
      /* istanbul ignore next */
      cov_5mnui7zlw().s[25]++;
      if (!isDeclared) {
        /* istanbul ignore next */
        cov_5mnui7zlw().b[4][0]++;
        cov_5mnui7zlw().s[26]++;
        validationResults.variableValidation.missing.push(detectedVar);
        /* istanbul ignore next */
        cov_5mnui7zlw().s[27]++;
        validationResults.warnings.push(`Variable '${detectedVar.name}' is used but not declared`);
      } else
      /* istanbul ignore next */
      {
        cov_5mnui7zlw().b[4][1]++;
      }
    }
    // Check for unused variable declarations
    /* istanbul ignore next */
    cov_5mnui7zlw().s[28]++;
    for (const declaredVar of declaredVariables) {
      const isUsed =
      /* istanbul ignore next */
      (cov_5mnui7zlw().s[29]++, detectedVariables.some(detected => {
        /* istanbul ignore next */
        cov_5mnui7zlw().f[3]++;
        cov_5mnui7zlw().s[30]++;
        return detected.name === declaredVar.name;
      }));
      /* istanbul ignore next */
      cov_5mnui7zlw().s[31]++;
      if (!isUsed) {
        /* istanbul ignore next */
        cov_5mnui7zlw().b[5][0]++;
        cov_5mnui7zlw().s[32]++;
        validationResults.variableValidation.unused.push(declaredVar);
        /* istanbul ignore next */
        cov_5mnui7zlw().s[33]++;
        validationResults.warnings.push(`Variable '${declaredVar.name}' is declared but not used`);
      } else
      /* istanbul ignore next */
      {
        cov_5mnui7zlw().b[5][1]++;
      }
    }
    // WhatsApp template validation
    /* istanbul ignore next */
    cov_5mnui7zlw().s[34]++;
    if (whatsappTemplate) {
      /* istanbul ignore next */
      cov_5mnui7zlw().b[6][0]++;
      cov_5mnui7zlw().s[35]++;
      validationResults.whatsappValidation = validateWhatsAppTemplate(whatsappTemplate);
      /* istanbul ignore next */
      cov_5mnui7zlw().s[36]++;
      if (validationResults.whatsappValidation.errors.length > 0) {
        /* istanbul ignore next */
        cov_5mnui7zlw().b[7][0]++;
        cov_5mnui7zlw().s[37]++;
        validationResults.isValid = false;
        /* istanbul ignore next */
        cov_5mnui7zlw().s[38]++;
        validationResults.errors.push(...validationResults.whatsappValidation.errors);
      } else
      /* istanbul ignore next */
      {
        cov_5mnui7zlw().b[7][1]++;
      }
      cov_5mnui7zlw().s[39]++;
      validationResults.warnings.push(...validationResults.whatsappValidation.warnings);
      /* istanbul ignore next */
      cov_5mnui7zlw().s[40]++;
      validationResults.suggestions.push(...validationResults.whatsappValidation.suggestions);
    } else
    /* istanbul ignore next */
    {
      cov_5mnui7zlw().b[6][1]++;
    }
    // Content quality suggestions
    const qualitySuggestions =
    /* istanbul ignore next */
    (cov_5mnui7zlw().s[41]++, generateQualitySuggestions(content));
    /* istanbul ignore next */
    cov_5mnui7zlw().s[42]++;
    validationResults.suggestions.push(...qualitySuggestions);
    /* istanbul ignore next */
    cov_5mnui7zlw().s[43]++;
    return (0, _apiutils.createSuccessResponse)(validationResults);
  } catch (error) {
    /* istanbul ignore next */
    cov_5mnui7zlw().s[44]++;
    console.error('Template validation error:', error);
    /* istanbul ignore next */
    cov_5mnui7zlw().s[45]++;
    return (0, _apiutils.createErrorResponse)(error);
  }
}
function extractVariablesFromContent(content) {
  /* istanbul ignore next */
  cov_5mnui7zlw().f[4]++;
  const variables =
  /* istanbul ignore next */
  (cov_5mnui7zlw().s[46]++, []);
  // Match {{variable}} pattern (WhatsApp style)
  const whatsappMatches =
  /* istanbul ignore next */
  (cov_5mnui7zlw().s[47]++, content.matchAll(/\{\{(\w+)\}\}/g));
  /* istanbul ignore next */
  cov_5mnui7zlw().s[48]++;
  for (const match of whatsappMatches) {
    /* istanbul ignore next */
    cov_5mnui7zlw().s[49]++;
    variables.push({
      name: match[1],
      position: match.index,
      type: 'whatsapp'
    });
  }
  // Match {variable} pattern (simple style)
  const simpleMatches =
  /* istanbul ignore next */
  (cov_5mnui7zlw().s[50]++, content.matchAll(/\{(\w+)\}/g));
  /* istanbul ignore next */
  cov_5mnui7zlw().s[51]++;
  for (const match of simpleMatches) {
    // Skip if already captured as WhatsApp variable
    const isWhatsApp =
    /* istanbul ignore next */
    (cov_5mnui7zlw().s[52]++, variables.some(v => {
      /* istanbul ignore next */
      cov_5mnui7zlw().f[5]++;
      cov_5mnui7zlw().s[53]++;
      return /* istanbul ignore next */(cov_5mnui7zlw().b[8][0]++, v.position <= match.index) &&
      /* istanbul ignore next */
      (cov_5mnui7zlw().b[8][1]++, v.position + v.name.length + 4 >= match.index + match[0].length);
    }));
    /* istanbul ignore next */
    cov_5mnui7zlw().s[54]++;
    if (!isWhatsApp) {
      /* istanbul ignore next */
      cov_5mnui7zlw().b[9][0]++;
      cov_5mnui7zlw().s[55]++;
      variables.push({
        name: match[1],
        position: match.index,
        type: 'simple'
      });
    } else
    /* istanbul ignore next */
    {
      cov_5mnui7zlw().b[9][1]++;
    }
  }
  /* istanbul ignore next */
  cov_5mnui7zlw().s[56]++;
  return variables;
}
function validateWhatsAppTemplate(whatsappTemplate) {
  /* istanbul ignore next */
  cov_5mnui7zlw().f[6]++;
  const validation =
  /* istanbul ignore next */
  (cov_5mnui7zlw().s[57]++, {
    isValid: true,
    errors: [],
    warnings: [],
    suggestions: []
  });
  // Required fields
  /* istanbul ignore next */
  cov_5mnui7zlw().s[58]++;
  if (!whatsappTemplate.name) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[10][0]++;
    cov_5mnui7zlw().s[59]++;
    validation.errors.push('WhatsApp template name is required');
    /* istanbul ignore next */
    cov_5mnui7zlw().s[60]++;
    validation.isValid = false;
  } else {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[10][1]++;
    cov_5mnui7zlw().s[61]++;
    // Name validation
    if (!/^[a-z0-9_]+$/.test(whatsappTemplate.name)) {
      /* istanbul ignore next */
      cov_5mnui7zlw().b[11][0]++;
      cov_5mnui7zlw().s[62]++;
      validation.errors.push('WhatsApp template name must contain only lowercase letters, numbers, and underscores');
      /* istanbul ignore next */
      cov_5mnui7zlw().s[63]++;
      validation.isValid = false;
    } else
    /* istanbul ignore next */
    {
      cov_5mnui7zlw().b[11][1]++;
    }
    cov_5mnui7zlw().s[64]++;
    if (whatsappTemplate.name.length > 512) {
      /* istanbul ignore next */
      cov_5mnui7zlw().b[12][0]++;
      cov_5mnui7zlw().s[65]++;
      validation.errors.push('WhatsApp template name must be 512 characters or less');
      /* istanbul ignore next */
      cov_5mnui7zlw().s[66]++;
      validation.isValid = false;
    } else
    /* istanbul ignore next */
    {
      cov_5mnui7zlw().b[12][1]++;
    }
  }
  /* istanbul ignore next */
  cov_5mnui7zlw().s[67]++;
  if (!whatsappTemplate.language) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[13][0]++;
    cov_5mnui7zlw().s[68]++;
    validation.errors.push('WhatsApp template language is required');
    /* istanbul ignore next */
    cov_5mnui7zlw().s[69]++;
    validation.isValid = false;
  } else
  /* istanbul ignore next */
  {
    cov_5mnui7zlw().b[13][1]++;
  }
  cov_5mnui7zlw().s[70]++;
  if (!whatsappTemplate.category) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[14][0]++;
    cov_5mnui7zlw().s[71]++;
    validation.errors.push('WhatsApp template category is required');
    /* istanbul ignore next */
    cov_5mnui7zlw().s[72]++;
    validation.isValid = false;
  } else {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[14][1]++;
    const validCategories =
    /* istanbul ignore next */
    (cov_5mnui7zlw().s[73]++, ['AUTHENTICATION', 'MARKETING', 'UTILITY']);
    /* istanbul ignore next */
    cov_5mnui7zlw().s[74]++;
    if (!validCategories.includes(whatsappTemplate.category)) {
      /* istanbul ignore next */
      cov_5mnui7zlw().b[15][0]++;
      cov_5mnui7zlw().s[75]++;
      validation.errors.push(`WhatsApp template category must be one of: ${validCategories.join(', ')}`);
      /* istanbul ignore next */
      cov_5mnui7zlw().s[76]++;
      validation.isValid = false;
    } else
    /* istanbul ignore next */
    {
      cov_5mnui7zlw().b[15][1]++;
    }
  }
  // Components validation
  /* istanbul ignore next */
  cov_5mnui7zlw().s[77]++;
  if (
  /* istanbul ignore next */
  (cov_5mnui7zlw().b[17][0]++, !whatsappTemplate.components) ||
  /* istanbul ignore next */
  (cov_5mnui7zlw().b[17][1]++, !Array.isArray(whatsappTemplate.components))) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[16][0]++;
    cov_5mnui7zlw().s[78]++;
    validation.errors.push('WhatsApp template components are required');
    /* istanbul ignore next */
    cov_5mnui7zlw().s[79]++;
    validation.isValid = false;
  } else {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[16][1]++;
    const hasBodyComponent =
    /* istanbul ignore next */
    (cov_5mnui7zlw().s[80]++, whatsappTemplate.components.some(c => {
      /* istanbul ignore next */
      cov_5mnui7zlw().f[7]++;
      cov_5mnui7zlw().s[81]++;
      return c.type === 'BODY';
    }));
    /* istanbul ignore next */
    cov_5mnui7zlw().s[82]++;
    if (!hasBodyComponent) {
      /* istanbul ignore next */
      cov_5mnui7zlw().b[18][0]++;
      cov_5mnui7zlw().s[83]++;
      validation.errors.push('WhatsApp template must have at least one BODY component');
      /* istanbul ignore next */
      cov_5mnui7zlw().s[84]++;
      validation.isValid = false;
    } else
    /* istanbul ignore next */
    {
      cov_5mnui7zlw().b[18][1]++;
    }
    // Validate each component
    cov_5mnui7zlw().s[85]++;
    for (const [index, component] of whatsappTemplate.components.entries()) {
      /* istanbul ignore next */
      cov_5mnui7zlw().s[86]++;
      if (!component.type) {
        /* istanbul ignore next */
        cov_5mnui7zlw().b[19][0]++;
        cov_5mnui7zlw().s[87]++;
        validation.errors.push(`Component ${index + 1}: type is required`);
        /* istanbul ignore next */
        cov_5mnui7zlw().s[88]++;
        validation.isValid = false;
        /* istanbul ignore next */
        cov_5mnui7zlw().s[89]++;
        continue;
      } else
      /* istanbul ignore next */
      {
        cov_5mnui7zlw().b[19][1]++;
      }
      const validComponentTypes =
      /* istanbul ignore next */
      (cov_5mnui7zlw().s[90]++, ['HEADER', 'BODY', 'FOOTER', 'BUTTONS']);
      /* istanbul ignore next */
      cov_5mnui7zlw().s[91]++;
      if (!validComponentTypes.includes(component.type)) {
        /* istanbul ignore next */
        cov_5mnui7zlw().b[20][0]++;
        cov_5mnui7zlw().s[92]++;
        validation.errors.push(`Component ${index + 1}: invalid type '${component.type}'`);
        /* istanbul ignore next */
        cov_5mnui7zlw().s[93]++;
        validation.isValid = false;
      } else
      /* istanbul ignore next */
      {
        cov_5mnui7zlw().b[20][1]++;
      }
      // Type-specific validations
      cov_5mnui7zlw().s[94]++;
      switch (component.type) {
        case 'BODY':
          /* istanbul ignore next */
          cov_5mnui7zlw().b[21][0]++;
          cov_5mnui7zlw().s[95]++;
          if (!component.text) {
            /* istanbul ignore next */
            cov_5mnui7zlw().b[22][0]++;
            cov_5mnui7zlw().s[96]++;
            validation.errors.push(`Component ${index + 1}: BODY component must have text`);
            /* istanbul ignore next */
            cov_5mnui7zlw().s[97]++;
            validation.isValid = false;
          } else {
            /* istanbul ignore next */
            cov_5mnui7zlw().b[22][1]++;
            cov_5mnui7zlw().s[98]++;
            if (component.text.length > 1024) {
              /* istanbul ignore next */
              cov_5mnui7zlw().b[23][0]++;
              cov_5mnui7zlw().s[99]++;
              validation.errors.push(`Component ${index + 1}: BODY text exceeds 1024 character limit`);
              /* istanbul ignore next */
              cov_5mnui7zlw().s[100]++;
              validation.isValid = false;
            } else
            /* istanbul ignore next */
            {
              cov_5mnui7zlw().b[23][1]++;
            }
          }
          /* istanbul ignore next */
          cov_5mnui7zlw().s[101]++;
          break;
        case 'HEADER':
          /* istanbul ignore next */
          cov_5mnui7zlw().b[21][1]++;
          cov_5mnui7zlw().s[102]++;
          if (
          /* istanbul ignore next */
          (cov_5mnui7zlw().b[25][0]++, component.format === 'TEXT') &&
          /* istanbul ignore next */
          (cov_5mnui7zlw().b[25][1]++, !component.text)) {
            /* istanbul ignore next */
            cov_5mnui7zlw().b[24][0]++;
            cov_5mnui7zlw().s[103]++;
            validation.errors.push(`Component ${index + 1}: TEXT HEADER must have text`);
            /* istanbul ignore next */
            cov_5mnui7zlw().s[104]++;
            validation.isValid = false;
          } else {
            /* istanbul ignore next */
            cov_5mnui7zlw().b[24][1]++;
            cov_5mnui7zlw().s[105]++;
            if (
            /* istanbul ignore next */
            (cov_5mnui7zlw().b[27][0]++, component.format === 'TEXT') &&
            /* istanbul ignore next */
            (cov_5mnui7zlw().b[27][1]++, component.text.length > 60)) {
              /* istanbul ignore next */
              cov_5mnui7zlw().b[26][0]++;
              cov_5mnui7zlw().s[106]++;
              validation.errors.push(`Component ${index + 1}: HEADER text exceeds 60 character limit`);
              /* istanbul ignore next */
              cov_5mnui7zlw().s[107]++;
              validation.isValid = false;
            } else
            /* istanbul ignore next */
            {
              cov_5mnui7zlw().b[26][1]++;
            }
          }
          /* istanbul ignore next */
          cov_5mnui7zlw().s[108]++;
          break;
        case 'FOOTER':
          /* istanbul ignore next */
          cov_5mnui7zlw().b[21][2]++;
          cov_5mnui7zlw().s[109]++;
          if (!component.text) {
            /* istanbul ignore next */
            cov_5mnui7zlw().b[28][0]++;
            cov_5mnui7zlw().s[110]++;
            validation.errors.push(`Component ${index + 1}: FOOTER component must have text`);
            /* istanbul ignore next */
            cov_5mnui7zlw().s[111]++;
            validation.isValid = false;
          } else {
            /* istanbul ignore next */
            cov_5mnui7zlw().b[28][1]++;
            cov_5mnui7zlw().s[112]++;
            if (component.text.length > 60) {
              /* istanbul ignore next */
              cov_5mnui7zlw().b[29][0]++;
              cov_5mnui7zlw().s[113]++;
              validation.errors.push(`Component ${index + 1}: FOOTER text exceeds 60 character limit`);
              /* istanbul ignore next */
              cov_5mnui7zlw().s[114]++;
              validation.isValid = false;
            } else
            /* istanbul ignore next */
            {
              cov_5mnui7zlw().b[29][1]++;
            }
          }
          /* istanbul ignore next */
          cov_5mnui7zlw().s[115]++;
          break;
        case 'BUTTONS':
          /* istanbul ignore next */
          cov_5mnui7zlw().b[21][3]++;
          cov_5mnui7zlw().s[116]++;
          if (
          /* istanbul ignore next */
          (cov_5mnui7zlw().b[31][0]++, !component.buttons) ||
          /* istanbul ignore next */
          (cov_5mnui7zlw().b[31][1]++, !Array.isArray(component.buttons))) {
            /* istanbul ignore next */
            cov_5mnui7zlw().b[30][0]++;
            cov_5mnui7zlw().s[117]++;
            validation.errors.push(`Component ${index + 1}: BUTTONS component must have buttons array`);
            /* istanbul ignore next */
            cov_5mnui7zlw().s[118]++;
            validation.isValid = false;
          } else {
            /* istanbul ignore next */
            cov_5mnui7zlw().b[30][1]++;
            cov_5mnui7zlw().s[119]++;
            if (component.buttons.length > 3) {
              /* istanbul ignore next */
              cov_5mnui7zlw().b[32][0]++;
              cov_5mnui7zlw().s[120]++;
              validation.errors.push(`Component ${index + 1}: Maximum 3 buttons allowed`);
              /* istanbul ignore next */
              cov_5mnui7zlw().s[121]++;
              validation.isValid = false;
            } else
            /* istanbul ignore next */
            {
              cov_5mnui7zlw().b[32][1]++;
            }
          }
          /* istanbul ignore next */
          cov_5mnui7zlw().s[122]++;
          break;
      }
    }
  }
  // Category-specific validations
  /* istanbul ignore next */
  cov_5mnui7zlw().s[123]++;
  if (whatsappTemplate.category === 'MARKETING') {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[33][0]++;
    cov_5mnui7zlw().s[124]++;
    validation.warnings.push('Marketing templates require pre-approval and may take longer to approve');
  } else
  /* istanbul ignore next */
  {
    cov_5mnui7zlw().b[33][1]++;
  }
  cov_5mnui7zlw().s[125]++;
  if (whatsappTemplate.category === 'AUTHENTICATION') {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[34][0]++;
    cov_5mnui7zlw().s[126]++;
    validation.suggestions.push('Authentication templates should include clear instructions for the user');
  } else
  /* istanbul ignore next */
  {
    cov_5mnui7zlw().b[34][1]++;
  }
  // General suggestions
  cov_5mnui7zlw().s[127]++;
  if (whatsappTemplate.components) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[35][0]++;
    const bodyComponent =
    /* istanbul ignore next */
    (cov_5mnui7zlw().s[128]++, whatsappTemplate.components.find(c => {
      /* istanbul ignore next */
      cov_5mnui7zlw().f[8]++;
      cov_5mnui7zlw().s[129]++;
      return c.type === 'BODY';
    }));
    /* istanbul ignore next */
    cov_5mnui7zlw().s[130]++;
    if (
    /* istanbul ignore next */
    (cov_5mnui7zlw().b[37][0]++, bodyComponent) &&
    /* istanbul ignore next */
    (cov_5mnui7zlw().b[37][1]++, bodyComponent.text)) {
      /* istanbul ignore next */
      cov_5mnui7zlw().b[36][0]++;
      cov_5mnui7zlw().s[131]++;
      if (bodyComponent.text.length < 20) {
        /* istanbul ignore next */
        cov_5mnui7zlw().b[38][0]++;
        cov_5mnui7zlw().s[132]++;
        validation.suggestions.push('Consider adding more context to make the message clearer');
      } else
      /* istanbul ignore next */
      {
        cov_5mnui7zlw().b[38][1]++;
      }
      cov_5mnui7zlw().s[133]++;
      if (!/[.!?]$/.test(bodyComponent.text.trim())) {
        /* istanbul ignore next */
        cov_5mnui7zlw().b[39][0]++;
        cov_5mnui7zlw().s[134]++;
        validation.suggestions.push('Consider ending the message with proper punctuation');
      } else
      /* istanbul ignore next */
      {
        cov_5mnui7zlw().b[39][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_5mnui7zlw().b[36][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_5mnui7zlw().b[35][1]++;
  }
  cov_5mnui7zlw().s[135]++;
  return validation;
}
function generateQualitySuggestions(content) {
  /* istanbul ignore next */
  cov_5mnui7zlw().f[9]++;
  const suggestions =
  /* istanbul ignore next */
  (cov_5mnui7zlw().s[136]++, []);
  // Length suggestions
  /* istanbul ignore next */
  cov_5mnui7zlw().s[137]++;
  if (content.length < 20) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[40][0]++;
    cov_5mnui7zlw().s[138]++;
    suggestions.push('Consider adding more context to make the message clearer');
  } else
  /* istanbul ignore next */
  {
    cov_5mnui7zlw().b[40][1]++;
  }
  cov_5mnui7zlw().s[139]++;
  if (content.length > 800) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[41][0]++;
    cov_5mnui7zlw().s[140]++;
    suggestions.push('Consider shortening the message for better readability');
  } else
  /* istanbul ignore next */
  {
    cov_5mnui7zlw().b[41][1]++;
  }
  // Formatting suggestions
  cov_5mnui7zlw().s[141]++;
  if (!/[.!?]$/.test(content.trim())) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[42][0]++;
    cov_5mnui7zlw().s[142]++;
    suggestions.push('Consider ending the message with proper punctuation');
  } else
  /* istanbul ignore next */
  {
    cov_5mnui7zlw().b[42][1]++;
  }
  cov_5mnui7zlw().s[143]++;
  if (content.includes('  ')) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[43][0]++;
    cov_5mnui7zlw().s[144]++;
    suggestions.push('Remove extra spaces for cleaner formatting');
  } else
  /* istanbul ignore next */
  {
    cov_5mnui7zlw().b[43][1]++;
  }
  // Content suggestions
  cov_5mnui7zlw().s[145]++;
  if (!/\b(please|thank|welcome|hello|hi)\b/i.test(content)) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[44][0]++;
    cov_5mnui7zlw().s[146]++;
    suggestions.push('Consider adding polite language to improve user experience');
  } else
  /* istanbul ignore next */
  {
    cov_5mnui7zlw().b[44][1]++;
  }
  cov_5mnui7zlw().s[147]++;
  if (
  /* istanbul ignore next */
  (cov_5mnui7zlw().b[46][0]++, content.toUpperCase() === content) &&
  /* istanbul ignore next */
  (cov_5mnui7zlw().b[46][1]++, content.length > 10)) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[45][0]++;
    cov_5mnui7zlw().s[148]++;
    suggestions.push('Avoid using ALL CAPS as it may appear aggressive');
  } else
  /* istanbul ignore next */
  {
    cov_5mnui7zlw().b[45][1]++;
  }
  // Variable usage suggestions
  const variableCount =
  /* istanbul ignore next */
  (cov_5mnui7zlw().s[149]++, (
  /* istanbul ignore next */
  (cov_5mnui7zlw().b[47][0]++, content.match(/\{[\w]+\}/g)) ||
  /* istanbul ignore next */
  (cov_5mnui7zlw().b[47][1]++, [])).length);
  /* istanbul ignore next */
  cov_5mnui7zlw().s[150]++;
  if (variableCount > 5) {
    /* istanbul ignore next */
    cov_5mnui7zlw().b[48][0]++;
    cov_5mnui7zlw().s[151]++;
    suggestions.push('Consider reducing the number of variables for simpler template management');
  } else
  /* istanbul ignore next */
  {
    cov_5mnui7zlw().b[48][1]++;
  }
  cov_5mnui7zlw().s[152]++;
  return suggestions;
}
