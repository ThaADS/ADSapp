      (cov_1jvltvaz8o().b[11][1]++, 0));
      const averageResponseTime =
      /* istanbul ignore next */
      (cov_1jvltvaz8o().s[49]++, performanceData.length > 0 ?
      /* istanbul ignore next */
      (cov_1jvltvaz8o().b[12][0]++, performanceData.reduce((sum, metric) => {
        /* istanbul ignore next */
        cov_1jvltvaz8o().f[11]++;
        cov_1jvltvaz8o().s[50]++;
        return sum + metric.duration_ms;
      }, 0) / performanceData.length) :
      /* istanbul ignore next */
      (cov_1jvltvaz8o().b[12][1]++, 0));
      // Calculate error rate as percentage
      const totalRequests =
      /* istanbul ignore next */
      (cov_1jvltvaz8o().s[51]++, performanceData.length);
      const errorRate =
      /* istanbul ignore next */
      (cov_1jvltvaz8o().s[52]++, totalRequests > 0 ?
      /* istanbul ignore next */
      (cov_1jvltvaz8o().b[13][0]++, errorCount / totalRequests * 100) :
      /* istanbul ignore next */
      (cov_1jvltvaz8o().b[13][1]++, 0));
      // Simple uptime calculation (percentage of successful requests)
      const uptime =
      /* istanbul ignore next */
      (cov_1jvltvaz8o().s[53]++, totalRequests > 0 ?
      /* istanbul ignore next */
      (cov_1jvltvaz8o().b[14][0]++, Math.max(0, 100 - errorRate)) :
      /* istanbul ignore next */
      (cov_1jvltvaz8o().b[14][1]++, 100));
      /* istanbul ignore next */
      cov_1jvltvaz8o().s[54]++;
      return {
        errorRate: Math.round(errorRate * 100) / 100,
        averageResponseTime: Math.round(averageResponseTime),
        activeAlerts: alertCount,
        uptime: Math.round(uptime * 100) / 100
      };
    } catch (error) {
      /* istanbul ignore next */
      cov_1jvltvaz8o().s[55]++;
      console.error('Failed to get health metrics:', error);
      /* istanbul ignore next */
      cov_1jvltvaz8o().s[56]++;
      return {
        errorRate: 0,
        averageResponseTime: 0,
        activeAlerts: 0,
        uptime: 100
      };
    }
  }
  async resolveAlert(alertId) {
    /* istanbul ignore next */
    cov_1jvltvaz8o().f[12]++;
    cov_1jvltvaz8o().s[57]++;
    try {
      const supabase =
      /* istanbul ignore next */
      (cov_1jvltvaz8o().s[58]++, await this.supabase);
      /* istanbul ignore next */
      cov_1jvltvaz8o().s[59]++;
      await supabase.from('system_alerts').update({
        is_resolved: true,
        resolved_at: new Date().toISOString()
      }).eq('id', alertId);
    } catch (error) {
      /* istanbul ignore next */
      cov_1jvltvaz8o().s[60]++;
      console.error('Failed to resolve alert:', error);
    }
  }
  async getErrorTrends(organizationId, days =
  /* istanbul ignore next */
  (cov_1jvltvaz8o().b[15][0]++, 7)) {
    /* istanbul ignore next */
    cov_1jvltvaz8o().f[13]++;
    cov_1jvltvaz8o().s[61]++;
    try {
      const supabase =
      /* istanbul ignore next */
      (cov_1jvltvaz8o().s[62]++, await this.supabase);
      const trends =
      /* istanbul ignore next */
      (cov_1jvltvaz8o().s[63]++, []);
      /* istanbul ignore next */
      cov_1jvltvaz8o().s[64]++;
      for (let i =
      /* istanbul ignore next */
      (cov_1jvltvaz8o().s[65]++, days - 1); i >= 0; i--) {
        const date =
        /* istanbul ignore next */
        (cov_1jvltvaz8o().s[66]++, new Date(Date.now() - i * 24 * 60 * 60 * 1000));
        const dateStr =
        /* istanbul ignore next */
        (cov_1jvltvaz8o().s[67]++, date.toISOString().split('T')[0]);
        const startOfDay =
        /* istanbul ignore next */
        (cov_1jvltvaz8o().s[68]++, `${dateStr}T00:00:00.000Z`);
        const endOfDay =
        /* istanbul ignore next */
        (cov_1jvltvaz8o().s[69]++, `${dateStr}T23:59:59.999Z`);
        let errorQuery =
        /* istanbul ignore next */
        (cov_1jvltvaz8o().s[70]++, supabase.from('error_logs').select('id', {
          count: 'exact'
        }).gte('timestamp', startOfDay).lte('timestamp', endOfDay));
        let performanceQuery =
        /* istanbul ignore next */
        (cov_1jvltvaz8o().s[71]++, supabase.from('performance_metrics').select('duration_ms').gte('timestamp', startOfDay).lte('timestamp', endOfDay));
        /* istanbul ignore next */
        cov_1jvltvaz8o().s[72]++;
        if (organizationId) {
          /* istanbul ignore next */
          cov_1jvltvaz8o().b[16][0]++;
          cov_1jvltvaz8o().s[73]++;
          errorQuery = errorQuery.eq('organization_id', organizationId);
          /* istanbul ignore next */
          cov_1jvltvaz8o().s[74]++;
          performanceQuery = performanceQuery.eq('organization_id', organizationId);
        } else
        /* istanbul ignore next */
        {
          cov_1jvltvaz8o().b[16][1]++;
        }
        const [errorResult, performanceResult] =
        /* istanbul ignore next */
        (cov_1jvltvaz8o().s[75]++, await Promise.all([errorQuery, performanceQuery]));
        const errorCount =
        /* istanbul ignore next */
        (cov_1jvltvaz8o().s[76]++,
        /* istanbul ignore next */
        (cov_1jvltvaz8o().b[17][0]++, errorResult.count) ||
        /* istanbul ignore next */
        (cov_1jvltvaz8o().b[17][1]++, 0));
        const performanceData =
        /* istanbul ignore next */
        (cov_1jvltvaz8o().s[77]++,
        /* istanbul ignore next */
        (cov_1jvltvaz8o().b[18][0]++, performanceResult.data) ||
        /* istanbul ignore next */
        (cov_1jvltvaz8o().b[18][1]++, []));
        const avgResponseTime =
        /* istanbul ignore next */
        (cov_1jvltvaz8o().s[78]++, performanceData.length > 0 ?
        /* istanbul ignore next */
        (cov_1jvltvaz8o().b[19][0]++, performanceData.reduce((sum, metric) => {
          /* istanbul ignore next */
          cov_1jvltvaz8o().f[14]++;
          cov_1jvltvaz8o().s[79]++;
          return sum + metric.duration_ms;
        }, 0) / performanceData.length) :
        /* istanbul ignore next */
        (cov_1jvltvaz8o().b[19][1]++, 0));
        /* istanbul ignore next */
        cov_1jvltvaz8o().s[80]++;
        trends.push({
          date: dateStr,
          errorCount,
          avgResponseTime: Math.round(avgResponseTime),
          totalRequests: performanceData.length
        });
      }
      /* istanbul ignore next */
      cov_1jvltvaz8o().s[81]++;
      return trends;
    } catch (error) {
      /* istanbul ignore next */
      cov_1jvltvaz8o().s[82]++;
      console.error('Failed to get error trends:', error);
      /* istanbul ignore next */
      cov_1jvltvaz8o().s[83]++;
      return [];
    }
  }
  async notifyExternalMonitoring(alert) {
    /* istanbul ignore next */
    cov_1jvltvaz8o().f[15]++;
    cov_1jvltvaz8o().s[84]++;
    try {
      /* istanbul ignore next */
      cov_1jvltvaz8o().s[85]++;
      // Integration with external monitoring services
      // This could be Sentry, DataDog, New Relic, etc.
      if (process.env.SENTRY_DSN) {
        /* istanbul ignore next */
        cov_1jvltvaz8o().b[20][0]++;
        cov_1jvltvaz8o().s[86]++;
        // Send to Sentry
        console.error('CRITICAL ALERT:', alert);
      } else
      /* istanbul ignore next */
      {
        cov_1jvltvaz8o().b[20][1]++;
      }
      cov_1jvltvaz8o().s[87]++;
      if (process.env.SLACK_WEBHOOK_URL) {
        /* istanbul ignore next */
        cov_1jvltvaz8o().b[21][0]++;
        cov_1jvltvaz8o().s[88]++;
        // Send to Slack
        await fetch(process.env.SLACK_WEBHOOK_URL, {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json'
          },
          body: JSON.stringify({
            text: ` CRITICAL ALERT: ${alert.message}`,
            attachments: [{
              color: 'danger',
              fields: [{
                title: 'Type',
                value: alert.type,
                short: true
              }, {
                title: 'Severity',
                value: alert.severity,
                short: true
              }, {
                title: 'Organization',
                value:
                /* istanbul ignore next */
                (cov_1jvltvaz8o().b[22][0]++, alert.organizationId) ||
                /* istanbul ignore next */
                (cov_1jvltvaz8o().b[22][1]++, 'System'),
                short: true
              }, {
                title: 'Timestamp',
                value: alert.timestamp,
                short: true
              }]
            }]
          })
        });
      } else
      /* istanbul ignore next */
      {
        cov_1jvltvaz8o().b[21][1]++;
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_1jvltvaz8o().s[89]++;
      console.error('Failed to notify external monitoring:', error);
    }
  }
}
const monitoring =
/* istanbul ignore next */
(cov_1jvltvaz8o().s[90]++, new MonitoringService());
async function logError(error) {
  /* istanbul ignore next */
  cov_1jvltvaz8o().f[16]++;
  cov_1jvltvaz8o().s[91]++;
  await monitoring.logError({
    type: 'system_error',
    severity: 'medium',
    timestamp: new Date().toISOString(),
    ...error
  });
}
