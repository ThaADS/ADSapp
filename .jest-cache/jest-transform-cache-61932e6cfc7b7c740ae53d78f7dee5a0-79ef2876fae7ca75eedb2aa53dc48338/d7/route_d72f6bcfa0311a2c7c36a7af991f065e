    }
    const {
      data: templates,
      error,
      count
    } =
    /* istanbul ignore next */
    (cov_1p25yw865s().s[29]++, await query.order('created_at', {
      ascending: false
    }).range(offset, offset + limit - 1));
    /* istanbul ignore next */
    cov_1p25yw865s().s[30]++;
    if (error) {
      /* istanbul ignore next */
      cov_1p25yw865s().b[4][0]++;
      cov_1p25yw865s().s[31]++;
      throw error;
    } else
    /* istanbul ignore next */
    {
      cov_1p25yw865s().b[4][1]++;
    }
    // Get usage statistics for each template
    const templatesWithStats =
    /* istanbul ignore next */
    (cov_1p25yw865s().s[32]++, await Promise.all((
    /* istanbul ignore next */
    (cov_1p25yw865s().b[5][0]++, templates) ||
    /* istanbul ignore next */
    (cov_1p25yw865s().b[5][1]++, [])).map(async template => {
      /* istanbul ignore next */
      cov_1p25yw865s().f[4]++;
      const {
        data: usage
      } =
      /* istanbul ignore next */
      (cov_1p25yw865s().s[33]++, await supabase.from('messages').select('id').eq('template_id', template.id));
      /* istanbul ignore next */
      cov_1p25yw865s().s[34]++;
      return {
        ...template,
        usageCount:
        /* istanbul ignore next */
        (cov_1p25yw865s().b[6][0]++, usage?.length) ||
        /* istanbul ignore next */
        (cov_1p25yw865s().b[6][1]++, 0)
      };
    })));
    /* istanbul ignore next */
    cov_1p25yw865s().s[35]++;
    return (0, _apiutils.createSuccessResponse)({
      templates: templatesWithStats,
      pagination: {
        page,
        limit,
        total:
        /* istanbul ignore next */
        (cov_1p25yw865s().b[7][0]++, count) ||
        /* istanbul ignore next */
        (cov_1p25yw865s().b[7][1]++, 0),
        hasMore: offset + limit < (
        /* istanbul ignore next */
        (cov_1p25yw865s().b[8][0]++, count) ||
        /* istanbul ignore next */
        (cov_1p25yw865s().b[8][1]++, 0))
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1p25yw865s().s[36]++;
    console.error('Error fetching templates:', error);
    /* istanbul ignore next */
    cov_1p25yw865s().s[37]++;
    return (0, _apiutils.createErrorResponse)(error);
  }
}
async function POST(request) {
  /* istanbul ignore next */
  cov_1p25yw865s().f[5]++;
