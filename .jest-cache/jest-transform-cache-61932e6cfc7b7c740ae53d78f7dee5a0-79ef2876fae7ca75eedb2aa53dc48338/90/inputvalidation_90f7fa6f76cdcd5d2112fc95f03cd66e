  if (Array.isArray(obj)) {
    /* istanbul ignore next */
    cov_11ykqkwph0().b[91][0]++;
    cov_11ykqkwph0().s[183]++;
    return obj.map(item => {
      /* istanbul ignore next */
      cov_11ykqkwph0().f[38]++;
      cov_11ykqkwph0().s[184]++;
      return sanitizeJSON(item, depth + 1);
    });
  } else
  /* istanbul ignore next */
  {
    cov_11ykqkwph0().b[91][1]++;
  }
  cov_11ykqkwph0().s[185]++;
  if (typeof obj === 'object') {
    /* istanbul ignore next */
    cov_11ykqkwph0().b[92][0]++;
    const sanitized =
    /* istanbul ignore next */
    (cov_11ykqkwph0().s[186]++, {});
