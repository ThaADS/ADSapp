    } else
    /* istanbul ignore next */
    {
      cov_11ykqkwph0().b[103][1]++;
    }
    const result =
    /* istanbul ignore next */
    (cov_11ykqkwph0().s[233]++, config.validator(value, config.options));
    /* istanbul ignore next */
    cov_11ykqkwph0().s[234]++;
