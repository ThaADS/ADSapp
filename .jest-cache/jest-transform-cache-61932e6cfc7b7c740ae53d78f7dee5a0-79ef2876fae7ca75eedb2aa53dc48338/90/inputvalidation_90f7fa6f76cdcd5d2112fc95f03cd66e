      /* istanbul ignore next */
      cov_11ykqkwph0().b[104][0]++;
      cov_11ykqkwph0().s[235]++;
      sanitizedData[key] = result.sanitizedValue;
    } else {
      /* istanbul ignore next */
      cov_11ykqkwph0().b[104][1]++;
      cov_11ykqkwph0().s[236]++;
      isValid = false;
      /* istanbul ignore next */
      cov_11ykqkwph0().s[237]++;
      errors[key] =
      /* istanbul ignore next */
      (cov_11ykqkwph0().b[105][0]++, result.error) ||
      /* istanbul ignore next */
      (cov_11ykqkwph0().b[105][1]++, 'Validation failed');
    }
  }
  /* istanbul ignore next */
  cov_11ykqkwph0().s[238]++;
  return {
    isValid,
    sanitizedData,
    errors
  };
}
