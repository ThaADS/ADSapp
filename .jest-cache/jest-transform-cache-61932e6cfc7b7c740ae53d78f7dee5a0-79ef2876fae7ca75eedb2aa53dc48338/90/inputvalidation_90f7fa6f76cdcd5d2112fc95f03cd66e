        /* istanbul ignore next */
        cov_11ykqkwph0().b[93][0]++;
        cov_11ykqkwph0().s[190]++;
        sanitized[sanitizedKey] = sanitizeText(value);
      } else {
        /* istanbul ignore next */
        cov_11ykqkwph0().b[93][1]++;
        cov_11ykqkwph0().s[191]++;
        if (typeof value === 'object') {
          /* istanbul ignore next */
          cov_11ykqkwph0().b[94][0]++;
          cov_11ykqkwph0().s[192]++;
