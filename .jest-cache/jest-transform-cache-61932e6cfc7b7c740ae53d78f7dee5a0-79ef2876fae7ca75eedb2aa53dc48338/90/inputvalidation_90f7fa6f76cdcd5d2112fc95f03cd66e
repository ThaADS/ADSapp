  .replace(/\\/g, '\\\\') // Escape backslashes
  .replace(/\0/g, '') // Remove null bytes
  .replace(/\n/g, ' ') // Replace newlines with spaces
  .replace(/\r/g, ' ') // Replace carriage returns with spaces
  .replace(/\t/g, ' ') // Replace tabs with spaces
  .replace(/[\x00-\x1F\x7F]/g, ''); // Remove control characters
}
function sanitizeJSON(obj, depth =
/* istanbul ignore next */
(cov_11ykqkwph0().b[87][0]++, 0)) {
  /* istanbul ignore next */
  cov_11ykqkwph0().f[37]++;
  cov_11ykqkwph0().s[178]++;
  // Prevent deep recursion attacks
  if (depth > 10) {
    /* istanbul ignore next */
    cov_11ykqkwph0().b[88][0]++;
    cov_11ykqkwph0().s[179]++;
    throw new Error('JSON nesting depth exceeded');
  } else
  /* istanbul ignore next */
  {
    cov_11ykqkwph0().b[88][1]++;
  }
  cov_11ykqkwph0().s[180]++;
  if (
  /* istanbul ignore next */
  (cov_11ykqkwph0().b[90][0]++, obj === null) ||
  /* istanbul ignore next */
  (cov_11ykqkwph0().b[90][1]++, obj === undefined)) {
    /* istanbul ignore next */
    cov_11ykqkwph0().b[89][0]++;
