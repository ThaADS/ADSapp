function sanitizeSearchQuery(query, maxLength =
/* istanbul ignore next */
(cov_11ykqkwph0().b[72][0]++, 100)) {
  /* istanbul ignore next */
  cov_11ykqkwph0().f[33]++;
  cov_11ykqkwph0().s[153]++;
  if (
  /* istanbul ignore next */
  (cov_11ykqkwph0().b[74][0]++, !query) ||
  /* istanbul ignore next */
  (cov_11ykqkwph0().b[74][1]++, typeof query !== 'string')) {
    /* istanbul ignore next */
    cov_11ykqkwph0().b[73][0]++;
    cov_11ykqkwph0().s[154]++;
    return '';
  } else
  /* istanbul ignore next */
  {
    cov_11ykqkwph0().b[73][1]++;
  }
  // Trim and limit length
  let sanitized =
  /* istanbul ignore next */
  (cov_11ykqkwph0().s[155]++, query.trim().substring(0, maxLength));
  // Remove SQL injection patterns
  /* istanbul ignore next */
  cov_11ykqkwph0().s[156]++;
  sanitized = sanitized.replace(/[';\\]/g, '') // Remove quotes and backslashes
  .replace(/--/g, '') // Remove SQL comments
  .replace(/\/\*/g, '') // Remove block comments
  .replace(/\*\//g, '').replace(/\bOR\b/gi, '') // Remove OR keyword
  .replace(/\bAND\b/gi, '') // Remove AND keyword
  .replace(/\bUNION\b/gi, '') // Remove UNION
  .replace(/\bSELECT\b/gi, '') // Remove SELECT
  .replace(/\bDROP\b/gi, '') // Remove DROP
  .replace(/\bDELETE\b/gi, '') // Remove DELETE
  .replace(/\bINSERT\b/gi, '') // Remove INSERT
  .replace(/\bUPDATE\b/gi, ''); // Remove UPDATE
  // Remove control characters
  /* istanbul ignore next */
  cov_11ykqkwph0().s[157]++;
  sanitized = sanitized.replace(/[\x00-\x1F\x7F]/g, '');
  // Escape regex special characters for safe pattern matching
  /* istanbul ignore next */
  cov_11ykqkwph0().s[158]++;
  sanitized = sanitized.replace(/[.*+?^${}()|[\]\\]/g, '\\$&');
  /* istanbul ignore next */
  cov_11ykqkwph0().s[159]++;
  return sanitized;
}
