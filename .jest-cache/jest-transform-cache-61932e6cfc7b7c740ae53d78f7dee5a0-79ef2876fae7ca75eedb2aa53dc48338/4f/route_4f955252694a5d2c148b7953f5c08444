      /* istanbul ignore next */
      cov_2qo6ed4o5q().b[2][0]++;
      cov_2qo6ed4o5q().s[27]++;
      return _server.NextResponse.json({
        error: 'Unauthorized'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_2qo6ed4o5q().b[2][1]++;
    }
    // Get user's organization
    const {
      data: profile
    } =
    /* istanbul ignore next */
    (cov_2qo6ed4o5q().s[28]++, await supabase.from('profiles').select('organization_id').eq('id', user.id).single());
    /* istanbul ignore next */
    cov_2qo6ed4o5q().s[29]++;
    if (!profile?.organization_id) {
      /* istanbul ignore next */
      cov_2qo6ed4o5q().b[3][0]++;
      cov_2qo6ed4o5q().s[30]++;
      return _server.NextResponse.json({
        error: 'No organization found'
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_2qo6ed4o5q().b[3][1]++;
    }
    // Update branding
    const brandingManager =
    /* istanbul ignore next */
    (cov_2qo6ed4o5q().s[31]++, new _tenantbranding.TenantBrandingManager(supabase));
    const updatedBranding =
    /* istanbul ignore next */
    (cov_2qo6ed4o5q().s[32]++, await brandingManager.updateBranding(profile.organization_id, body));
    /* istanbul ignore next */
    cov_2qo6ed4o5q().s[33]++;
    return _server.NextResponse.json({
      success: true,
      data: updatedBranding
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_2qo6ed4o5q().s[34]++;
    console.error('Error updating branding:', error);
    /* istanbul ignore next */
    cov_2qo6ed4o5q().s[35]++;
    return _server.NextResponse.json({
      error: 'Internal server error'
    }, {
      status: 500
    });
  }
}
