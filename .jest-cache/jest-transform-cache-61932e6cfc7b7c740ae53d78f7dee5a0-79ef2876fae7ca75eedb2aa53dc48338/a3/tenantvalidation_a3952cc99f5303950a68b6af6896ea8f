  (cov_wre5m29gw().b[5][0]++, obj === null) ||
  /* istanbul ignore next */
  (cov_wre5m29gw().b[5][1]++, typeof obj !== "object") &&
  /* istanbul ignore next */
  (cov_wre5m29gw().b[5][2]++, typeof obj !== "function")) {
    /* istanbul ignore next */
    cov_wre5m29gw().b[4][0]++;
    cov_wre5m29gw().s[21]++;
    return {
      default: obj
    };
  } else
  /* istanbul ignore next */
  {
    cov_wre5m29gw().b[4][1]++;
  }
  var cache =
  /* istanbul ignore next */
  (cov_wre5m29gw().s[22]++, _getRequireWildcardCache(nodeInterop));
  /* istanbul ignore next */
  cov_wre5m29gw().s[23]++;
  if (
  /* istanbul ignore next */
  (cov_wre5m29gw().b[7][0]++, cache) &&
  /* istanbul ignore next */
  (cov_wre5m29gw().b[7][1]++, cache.has(obj))) {
    /* istanbul ignore next */
    cov_wre5m29gw().b[6][0]++;
    cov_wre5m29gw().s[24]++;
    return cache.get(obj);
  } else
  /* istanbul ignore next */
  {
    cov_wre5m29gw().b[6][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_wre5m29gw().s[25]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_wre5m29gw().s[26]++,
  /* istanbul ignore next */
  (cov_wre5m29gw().b[8][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_wre5m29gw().b[8][1]++, Object.getOwnPropertyDescriptor));
  /* istanbul ignore next */
  cov_wre5m29gw().s[27]++;
  for (var key in obj) {
    /* istanbul ignore next */
    cov_wre5m29gw().s[28]++;
    if (
    /* istanbul ignore next */
    (cov_wre5m29gw().b[10][0]++, key !== "default") &&
    /* istanbul ignore next */
    (cov_wre5m29gw().b[10][1]++, Object.prototype.hasOwnProperty.call(obj, key))) {
      /* istanbul ignore next */
      cov_wre5m29gw().b[9][0]++;
      var desc =
      /* istanbul ignore next */
      (cov_wre5m29gw().s[29]++, hasPropertyDescriptor ?
      /* istanbul ignore next */
      (cov_wre5m29gw().b[11][0]++, Object.getOwnPropertyDescriptor(obj, key)) :
      /* istanbul ignore next */
      (cov_wre5m29gw().b[11][1]++, null));
      /* istanbul ignore next */
      cov_wre5m29gw().s[30]++;
      if (
      /* istanbul ignore next */
      (cov_wre5m29gw().b[13][0]++, desc) && (
      /* istanbul ignore next */
      (cov_wre5m29gw().b[13][1]++, desc.get) ||
      /* istanbul ignore next */
      (cov_wre5m29gw().b[13][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_wre5m29gw().b[12][0]++;
        cov_wre5m29gw().s[31]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_wre5m29gw().b[12][1]++;
        cov_wre5m29gw().s[32]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_wre5m29gw().b[9][1]++;
    }
  }
  /* istanbul ignore next */
  cov_wre5m29gw().s[33]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_wre5m29gw().s[34]++;
  if (cache) {
    /* istanbul ignore next */
    cov_wre5m29gw().b[14][0]++;
    cov_wre5m29gw().s[35]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_wre5m29gw().b[14][1]++;
  }
  cov_wre5m29gw().s[36]++;
  return newObj;
}
async function validateTenantAccess(request) {
  /* istanbul ignore next */
  cov_wre5m29gw().f[9]++;
  cov_wre5m29gw().s[37]++;
  try {
    const supabase =
    /* istanbul ignore next */
    (cov_wre5m29gw().s[38]++, await (0, _server1.createClient)());
    // 1. Verify user authentication
    const {
      data: {
        user
      },
      error: authError
    } =
    /* istanbul ignore next */
    (cov_wre5m29gw().s[39]++, await supabase.auth.getUser());
    /* istanbul ignore next */
    cov_wre5m29gw().s[40]++;
    if (
    /* istanbul ignore next */
    (cov_wre5m29gw().b[16][0]++, authError) ||
    /* istanbul ignore next */
    (cov_wre5m29gw().b[16][1]++, !user)) {
      /* istanbul ignore next */
      cov_wre5m29gw().b[15][0]++;
      cov_wre5m29gw().s[41]++;
      return _server.NextResponse.json({
        error: 'Authentication required',
        code: 'UNAUTHORIZED'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_wre5m29gw().b[15][1]++;
    }
    // 2. Get user's organization and profile
    const userOrg =
    /* istanbul ignore next */
    (cov_wre5m29gw().s[42]++, await (0, _apiutils.getUserOrganization)(user.id));
    /* istanbul ignore next */
    cov_wre5m29gw().s[43]++;
    if (
    /* istanbul ignore next */
    (cov_wre5m29gw().b[18][0]++, !userOrg) ||
    /* istanbul ignore next */
    (cov_wre5m29gw().b[18][1]++, !userOrg.organization_id)) {
      /* istanbul ignore next */
      cov_wre5m29gw().b[17][0]++;
      // Check if user is super admin - they don't need organization
      const {
        data: profile
      } =
      /* istanbul ignore next */
      (cov_wre5m29gw().s[44]++, await supabase.from('profiles').select('is_super_admin, role').eq('id', user.id).single());
      /* istanbul ignore next */
      cov_wre5m29gw().s[45]++;
      if (profile?.is_super_admin) {
        /* istanbul ignore next */
        cov_wre5m29gw().b[19][0]++;
        // Super admins bypass organization requirements
        const requestHeaders =
        /* istanbul ignore next */
        (cov_wre5m29gw().s[46]++, new Headers(request.headers));
        /* istanbul ignore next */
        cov_wre5m29gw().s[47]++;
        requestHeaders.set('x-user-id', user.id);
        /* istanbul ignore next */
        cov_wre5m29gw().s[48]++;
        requestHeaders.set('x-user-role', 'super_admin');
        /* istanbul ignore next */
        cov_wre5m29gw().s[49]++;
        requestHeaders.set('x-is-super-admin', 'true');
        /* istanbul ignore next */
        cov_wre5m29gw().s[50]++;
        requestHeaders.set('x-user-email',
        /* istanbul ignore next */
        (cov_wre5m29gw().b[20][0]++, user.email) ||
        /* istanbul ignore next */
        (cov_wre5m29gw().b[20][1]++, ''));
        /* istanbul ignore next */
        cov_wre5m29gw().s[51]++;
        return _server.NextResponse.next({
          request: {
            headers: requestHeaders
          }
        });
      } else
      /* istanbul ignore next */
      {
        cov_wre5m29gw().b[19][1]++;
      }
      cov_wre5m29gw().s[52]++;
      return _server.NextResponse.json({
        error: 'User not associated with any organization',
        code: 'NO_ORGANIZATION'
      }, {
        status: 403
      });
    } else
    /* istanbul ignore next */
    {
      cov_wre5m29gw().b[17][1]++;
    }
    // 3. Validate tenant context from request
    // Check for organization ID in headers or query parameters
    const requestedOrgId =
    /* istanbul ignore next */
    (cov_wre5m29gw().s[53]++,
    /* istanbul ignore next */
    (cov_wre5m29gw().b[21][0]++, request.headers.get('x-organization-id')) ||
    /* istanbul ignore next */
    (cov_wre5m29gw().b[21][1]++, request.nextUrl.searchParams.get('organization_id')));
    // 4. Cross-tenant access prevention
    /* istanbul ignore next */
    cov_wre5m29gw().s[54]++;
    if (
    /* istanbul ignore next */
    (cov_wre5m29gw().b[23][0]++, requestedOrgId) &&
    /* istanbul ignore next */
    (cov_wre5m29gw().b[23][1]++, requestedOrgId !== userOrg.organization_id)) {
      /* istanbul ignore next */
      cov_wre5m29gw().b[22][0]++;
      // Log cross-tenant access attempt for security monitoring
      const securityEvent =
      /* istanbul ignore next */
      (cov_wre5m29gw().s[55]++, {
        userId: user.id,
        userOrg: userOrg.organization_id,
        requestedOrg: requestedOrgId,
        path: request.nextUrl.pathname,
        method: request.method,
        timestamp: new Date().toISOString(),
        ip: getClientIp(request),
        userAgent:
        /* istanbul ignore next */
        (cov_wre5m29gw().b[24][0]++, request.headers.get('user-agent')) ||
        /* istanbul ignore next */
        (cov_wre5m29gw().b[24][1]++, 'unknown')
      });
      // Log to console (in production, send to security monitoring service)
      /* istanbul ignore next */
      cov_wre5m29gw().s[56]++;
      console.warn('[SECURITY] Cross-tenant access attempt:', securityEvent);
      // Send to Sentry for alerting
      /* istanbul ignore next */
      cov_wre5m29gw().s[57]++;
      if (process.env.NODE_ENV === 'production') {
        /* istanbul ignore next */
        cov_wre5m29gw().b[25][0]++;
        cov_wre5m29gw().s[58]++;
        try {
          const Sentry =
          /* istanbul ignore next */
          (cov_wre5m29gw().s[59]++, await Promise.resolve().then(() => {
            /* istanbul ignore next */
            cov_wre5m29gw().f[10]++;
            cov_wre5m29gw().s[60]++;
            return /*#__PURE__*/_interop_require_wildcard(require("@sentry/nextjs"));
          }));
          /* istanbul ignore next */
          cov_wre5m29gw().s[61]++;
          Sentry.captureMessage('Cross-tenant access attempt', {
            level: 'warning',
            extra: {
              userId: securityEvent.userId,
              userOrg: securityEvent.userOrg,
              requestedOrg: securityEvent.requestedOrg,
              path: securityEvent.path,
              method: securityEvent.method,
              timestamp: securityEvent.timestamp,
              ip: securityEvent.ip,
              userAgent: securityEvent.userAgent
            }
          });
        } catch (error) {
          /* istanbul ignore next */
          cov_wre5m29gw().s[62]++;
          console.error('Failed to log security event to Sentry:', error);
        }
      } else
      /* istanbul ignore next */
      {
        cov_wre5m29gw().b[25][1]++;
      }
      cov_wre5m29gw().s[63]++;
      return _server.NextResponse.json({
        error: 'Forbidden: Access to this organization denied',
        code: 'FORBIDDEN'
      }, {
        status: 403
      });
    } else
    /* istanbul ignore next */
    {
      cov_wre5m29gw().b[22][1]++;
    }
    // 5. Attach tenant context to request headers for downstream use
    const requestHeaders =
    /* istanbul ignore next */
    (cov_wre5m29gw().s[64]++, new Headers(request.headers));
    /* istanbul ignore next */
    cov_wre5m29gw().s[65]++;
    requestHeaders.set('x-user-id', user.id);
    /* istanbul ignore next */
    cov_wre5m29gw().s[66]++;
    requestHeaders.set('x-organization-id', userOrg.organization_id);
    // Type guard for organization role
    const orgRole =
    /* istanbul ignore next */
    (cov_wre5m29gw().s[67]++,
    /* istanbul ignore next */
    (cov_wre5m29gw().b[27][0]++, userOrg.organization) &&
    /* istanbul ignore next */
    (cov_wre5m29gw().b[27][1]++, typeof userOrg.organization === 'object') &&
    /* istanbul ignore next */
    (cov_wre5m29gw().b[27][2]++, 'role' in userOrg.organization) ?
    /* istanbul ignore next */
    (cov_wre5m29gw().b[26][0]++, String(userOrg.organization.role)) :
    /* istanbul ignore next */
    (cov_wre5m29gw().b[26][1]++, 'agent'));
    /* istanbul ignore next */
    cov_wre5m29gw().s[68]++;
    requestHeaders.set('x-user-role', orgRole);
    /* istanbul ignore next */
    cov_wre5m29gw().s[69]++;
    requestHeaders.set('x-user-email',
    /* istanbul ignore next */
    (cov_wre5m29gw().b[28][0]++, user.email) ||
    /* istanbul ignore next */
    (cov_wre5m29gw().b[28][1]++, ''));
    /* istanbul ignore next */
    cov_wre5m29gw().s[70]++;
    requestHeaders.set('x-is-super-admin', 'false');
    // Return modified request with tenant context
    /* istanbul ignore next */
    cov_wre5m29gw().s[71]++;
    return _server.NextResponse.next({
      request: {
        headers: requestHeaders
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_wre5m29gw().s[72]++;
    console.error('[TENANT_VALIDATION] Error:', error);
    // Log error to monitoring service
    /* istanbul ignore next */
    cov_wre5m29gw().s[73]++;
    if (process.env.NODE_ENV === 'production') {
      /* istanbul ignore next */
      cov_wre5m29gw().b[29][0]++;
      cov_wre5m29gw().s[74]++;
      try {
        const Sentry =
        /* istanbul ignore next */
        (cov_wre5m29gw().s[75]++, await Promise.resolve().then(() => {
          /* istanbul ignore next */
          cov_wre5m29gw().f[11]++;
          cov_wre5m29gw().s[76]++;
          return /*#__PURE__*/_interop_require_wildcard(require("@sentry/nextjs"));
        }));
        /* istanbul ignore next */
        cov_wre5m29gw().s[77]++;
        Sentry.captureException(error, {
          tags: {
            middleware: 'tenant-validation'
          }
        });
      } catch (sentryError) {
        /* istanbul ignore next */
        cov_wre5m29gw().s[78]++;
        console.error('Failed to log error to Sentry:', sentryError);
      }
    } else
    /* istanbul ignore next */
    {
      cov_wre5m29gw().b[29][1]++;
    }
    // Return 500 error for unexpected failures
    cov_wre5m29gw().s[79]++;
    return _server.NextResponse.json({
      error: 'Internal server error during tenant validation',
      code: 'INTERNAL_ERROR'
    }, {
      status: 500
    });
  }
}
function getTenantContext(request) {
  /* istanbul ignore next */
  cov_wre5m29gw().f[12]++;
  cov_wre5m29gw().s[80]++;
  return {
    userId:
    /* istanbul ignore next */
    (cov_wre5m29gw().b[30][0]++, request.headers.get('x-user-id')) ||
    /* istanbul ignore next */
    (cov_wre5m29gw().b[30][1]++, ''),
    organizationId:
    /* istanbul ignore next */
    (cov_wre5m29gw().b[31][0]++, request.headers.get('x-organization-id')) ||
    /* istanbul ignore next */
    (cov_wre5m29gw().b[31][1]++, ''),
    userRole:
    /* istanbul ignore next */
    (cov_wre5m29gw().b[32][0]++, request.headers.get('x-user-role')) ||
    /* istanbul ignore next */
    (cov_wre5m29gw().b[32][1]++, 'agent'),
    userEmail:
    /* istanbul ignore next */
    (cov_wre5m29gw().b[33][0]++, request.headers.get('x-user-email')) ||
    /* istanbul ignore next */
    (cov_wre5m29gw().b[33][1]++, '')
  };
}
function isSuperAdmin(request) {
  /* istanbul ignore next */
  cov_wre5m29gw().f[13]++;
  cov_wre5m29gw().s[81]++;
  return request.headers.get('x-is-super-admin') === 'true';
}
function validateResourceAccess(resourceOrgId, tenantContext) {
  /* istanbul ignore next */
  cov_wre5m29gw().f[14]++;
  cov_wre5m29gw().s[82]++;
  // Super admins can access all resources
  if (
  /* istanbul ignore next */
  (cov_wre5m29gw().b[35][0]++, !tenantContext.organizationId) &&
  /* istanbul ignore next */
  (cov_wre5m29gw().b[35][1]++, tenantContext.userRole === 'super_admin')) {
    /* istanbul ignore next */
    cov_wre5m29gw().b[34][0]++;
    cov_wre5m29gw().s[83]++;
    return true;
  } else
  /* istanbul ignore next */
  {
    cov_wre5m29gw().b[34][1]++;
  }
  // Regular users can only access resources in their organization
  cov_wre5m29gw().s[84]++;
  return resourceOrgId === tenantContext.organizationId;
}
/**
 * Helper function to get client IP address
 * Handles various proxy headers (Vercel, Cloudflare, etc.)
 *
 * @param request - NextRequest object
 * @returns Client IP address or 'unknown'
 */
function getClientIp(request) {
  /* istanbul ignore next */
  cov_wre5m29gw().f[15]++;
  const forwarded =
  /* istanbul ignore next */
  (cov_wre5m29gw().s[85]++, request.headers.get('x-forwarded-for'));
  const realIp =
  /* istanbul ignore next */
  (cov_wre5m29gw().s[86]++, request.headers.get('x-real-ip'));
  const cfConnectingIp =
  /* istanbul ignore next */
  (cov_wre5m29gw().s[87]++, request.headers.get('cf-connecting-ip'));
  const vercelIp =
  /* istanbul ignore next */
  (cov_wre5m29gw().s[88]++, request.headers.get('x-vercel-forwarded-for'));
  /* istanbul ignore next */
  cov_wre5m29gw().s[89]++;
  return /* istanbul ignore next */(cov_wre5m29gw().b[36][0]++, cfConnectingIp) ||
  /* istanbul ignore next */
  (cov_wre5m29gw().b[36][1]++, vercelIp) ||
  /* istanbul ignore next */
  (cov_wre5m29gw().b[36][2]++, forwarded?.split(',')[0].trim()) ||
  /* istanbul ignore next */
  (cov_wre5m29gw().b[36][3]++, realIp) ||
  /* istanbul ignore next */
  (cov_wre5m29gw().b[36][4]++, 'unknown');
}
function createTenantScope(organizationId) {
  /* istanbul ignore next */
  cov_wre5m29gw().f[16]++;
  cov_wre5m29gw().s[90]++;
  return query => {
    /* istanbul ignore next */
    cov_wre5m29gw().f[17]++;
    cov_wre5m29gw().s[91]++;
    return query.eq('organization_id', organizationId);
  };
}
