  }
  handleNewMessage(message) {
    /* istanbul ignore next */
    cov_2izbh9vhev().f[7]++;
    cov_2izbh9vhev().s[22]++;
    // Dispatch custom event for new messages
    window.dispatchEvent(new CustomEvent('newMessage', {
