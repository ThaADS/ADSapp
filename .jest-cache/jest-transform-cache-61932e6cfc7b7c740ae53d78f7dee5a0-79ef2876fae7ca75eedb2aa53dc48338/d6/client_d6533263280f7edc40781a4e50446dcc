      detail: conversation
    }));
  }
  scheduleReconnect() {
    /* istanbul ignore next */
    cov_2izbh9vhev().f[9]++;
    cov_2izbh9vhev().s[24]++;
    if (this.reconnectAttempts < this.maxReconnectAttempts) {
      /* istanbul ignore next */
      cov_2izbh9vhev().b[1][0]++;
      cov_2izbh9vhev().s[25]++;
      setTimeout(() => {
        /* istanbul ignore next */
        cov_2izbh9vhev().f[10]++;
        cov_2izbh9vhev().s[26]++;
        this.reconnectAttempts++;
        /* istanbul ignore next */
        cov_2izbh9vhev().s[27]++;
        this.connect();
      }, this.reconnectDelay * this.reconnectAttempts);
    } else
    /* istanbul ignore next */
    {
      cov_2izbh9vhev().b[1][1]++;
    }
  }
  disconnect() {
    /* istanbul ignore next */
    cov_2izbh9vhev().f[11]++;
    cov_2izbh9vhev().s[28]++;
    if (this.ws) {
      /* istanbul ignore next */
