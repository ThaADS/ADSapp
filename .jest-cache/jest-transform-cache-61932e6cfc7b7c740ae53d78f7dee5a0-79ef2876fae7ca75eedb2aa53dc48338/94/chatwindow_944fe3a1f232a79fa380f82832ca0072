      children: isLoading ?
      /* istanbul ignore next */
      (/*#__PURE__*/cov_bsmb1or2c().b[12][0]++, (0, _jsxruntime.jsx)("div", {
        className: "flex items-center justify-center h-full",
        children: /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
          className: "animate-spin rounded-full h-8 w-8 border-b-2 border-green-600"
        })
      })) :
      /* istanbul ignore next */
      (/*#__PURE__*/cov_bsmb1or2c().b[12][1]++, (0, _jsxruntime.jsx)(_messagelist.MessageList, {
        messages: messages,
        currentUserId: profile.id
      }))
    }), /*#__PURE__*/(0, _jsxruntime.jsx)(_messageinput.MessageInput, {
      onSendMessage: handleSendMessage
    })]
  });
}
