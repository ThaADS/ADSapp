function sanitizeObject(obj) {
  /* istanbul ignore next */
  cov_7bk2zx2jc().f[55]++;
  const sanitized =
  /* istanbul ignore next */
  (cov_7bk2zx2jc().s[98]++, {});
  /* istanbul ignore next */
  cov_7bk2zx2jc().s[99]++;
  for (const [key, value] of Object.entries(obj)) {
    /* istanbul ignore next */
    cov_7bk2zx2jc().s[100]++;
    if (typeof value === 'string') {
      /* istanbul ignore next */
      cov_7bk2zx2jc().b[8][0]++;
      cov_7bk2zx2jc().s[101]++;
      sanitized[key] = sanitizeInput(value);
