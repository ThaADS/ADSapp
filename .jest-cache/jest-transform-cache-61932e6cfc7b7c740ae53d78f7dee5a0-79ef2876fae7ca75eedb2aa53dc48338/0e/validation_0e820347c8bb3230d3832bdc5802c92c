    const validated =
    /* istanbul ignore next */
    (cov_7bk2zx2jc().s[85]++, schema.parse(data));
    /* istanbul ignore next */
    cov_7bk2zx2jc().s[86]++;
    return {
      success: true,
      data: validated
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_7bk2zx2jc().s[87]++;
    if (error instanceof _zod.z.ZodError) {
      /* istanbul ignore next */
      cov_7bk2zx2jc().b[7][0]++;
