    (cov_118kanpw0v().s[8]++, await (0, _server1.createClient)());
    const tenantContext =
    /* istanbul ignore next */
    (cov_118kanpw0v().s[9]++, _tenantrouting.tenantUtils.getTenantContext(request.headers));
    /* istanbul ignore next */
    cov_118kanpw0v().s[10]++;
    if (!tenantContext) {
      /* istanbul ignore next */
      cov_118kanpw0v().b[0][0]++;
      cov_118kanpw0v().s[11]++;
      return _server.NextResponse.json({
        error: 'Tenant not found'
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_118kanpw0v().b[0][1]++;
    }
    // Verify authentication and authorization
    const {
      data: {
        user
      },
      error: authError
    } =
    /* istanbul ignore next */
    (cov_118kanpw0v().s[12]++, await supabase.auth.getUser(
    /* istanbul ignore next */
    (cov_118kanpw0v().b[1][0]++, request.headers.get('authorization')?.replace('Bearer ', '')) ||
    /* istanbul ignore next */
    (cov_118kanpw0v().b[1][1]++, '')));
    /* istanbul ignore next */
    cov_118kanpw0v().s[13]++;
    if (
    /* istanbul ignore next */
    (cov_118kanpw0v().b[3][0]++, authError) ||
    /* istanbul ignore next */
    (cov_118kanpw0v().b[3][1]++, !user)) {
      /* istanbul ignore next */
      cov_118kanpw0v().b[2][0]++;
      cov_118kanpw0v().s[14]++;
      return _server.NextResponse.json({
        error: 'Unauthorized'
      }, {
        status: 401
      });
    } else
    /* istanbul ignore next */
    {
      cov_118kanpw0v().b[2][1]++;
    }
    const {
      data: profile,
      error: profileError
    } =
    /* istanbul ignore next */
    (cov_118kanpw0v().s[15]++, await supabase.from('profiles').select('organization_id, role').eq('id', user.id).single());
    /* istanbul ignore next */
    cov_118kanpw0v().s[16]++;
    if (
    /* istanbul ignore next */
    (cov_118kanpw0v().b[5][0]++, profileError) ||
    /* istanbul ignore next */
    (cov_118kanpw0v().b[5][1]++, profile.organization_id !== tenantContext.organizationId) ||
    /* istanbul ignore next */
    (cov_118kanpw0v().b[5][2]++, !['owner', 'admin'].includes(profile.role))) {
      /* istanbul ignore next */
      cov_118kanpw0v().b[4][0]++;
      cov_118kanpw0v().s[17]++;
      return _server.NextResponse.json({
        error: 'Forbidden'
      }, {
        status: 403
      });
    } else
    /* istanbul ignore next */
    {
      cov_118kanpw0v().b[4][1]++;
    }
    // Check if domain exists and belongs to organization
    const {
      data: domain,
      error: domainError
    } =
    /* istanbul ignore next */
    (cov_118kanpw0v().s[18]++, await supabase.from('tenant_domains').select('*').eq('id', id).eq('organization_id', tenantContext.organizationId).single());
    /* istanbul ignore next */
    cov_118kanpw0v().s[19]++;
    if (
    /* istanbul ignore next */
    (cov_118kanpw0v().b[7][0]++, domainError) ||
    /* istanbul ignore next */
    (cov_118kanpw0v().b[7][1]++, !domain)) {
      /* istanbul ignore next */
      cov_118kanpw0v().b[6][0]++;
      cov_118kanpw0v().s[20]++;
      return _server.NextResponse.json({
        error: 'Domain not found'
      }, {
        status: 404
      });
    } else
    /* istanbul ignore next */
    {
      cov_118kanpw0v().b[6][1]++;
    }
    // Only custom domains need verification
    cov_118kanpw0v().s[21]++;
    if (domain.domain_type !== 'custom') {
      /* istanbul ignore next */
      cov_118kanpw0v().b[8][0]++;
      cov_118kanpw0v().s[22]++;
      return _server.NextResponse.json({
        error: 'Only custom domains require verification'
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_118kanpw0v().b[8][1]++;
    }
    // Verify domain
    const domainManager =
    /* istanbul ignore next */
    (cov_118kanpw0v().s[23]++, new _tenantrouting.DomainManager(process.env.NEXT_PUBLIC_SUPABASE_URL, process.env.SUPABASE_SERVICE_ROLE_KEY));
    const isVerified =
    /* istanbul ignore next */
    (cov_118kanpw0v().s[24]++, await domainManager.verifyDomain(id));
    // Get updated domain data
    const {
      data: updatedDomain,
      error: updateError
    } =
    /* istanbul ignore next */
    (cov_118kanpw0v().s[25]++, await supabase.from('tenant_domains').select('*').eq('id', id).single());
    /* istanbul ignore next */
    cov_118kanpw0v().s[26]++;
    if (updateError) {
      /* istanbul ignore next */
      cov_118kanpw0v().b[9][0]++;
      cov_118kanpw0v().s[27]++;
      return _server.NextResponse.json({
        error: 'Failed to fetch updated domain'
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_118kanpw0v().b[9][1]++;
    }
    cov_118kanpw0v().s[28]++;
    return _server.NextResponse.json({
      success: true,
      data: {
        domain: updatedDomain,
        verified: isVerified,
        message: isVerified ?
        /* istanbul ignore next */
        (cov_118kanpw0v().b[10][0]++, 'Domain verified successfully') :
        /* istanbul ignore next */
        (cov_118kanpw0v().b[10][1]++, 'Domain verification failed. Please check your DNS settings.')
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_118kanpw0v().s[29]++;
    console.error('Error verifying domain:', error);
    /* istanbul ignore next */
    cov_118kanpw0v().s[30]++;
    return _server.NextResponse.json({
      error: 'Internal server error'
    }, {
      status: 500
    });
  }
}
