      return /* istanbul ignore next */(cov_ns6b9lara().b[18][0]++, resolved === undefined) ||
      /* istanbul ignore next */
      (cov_ns6b9lara().b[18][1]++, alert.resolved === resolved);
    });
  }
  resolveAlert(alertId) {
    /* istanbul ignore next */
    cov_ns6b9lara().f[35]++;
    const alert =
    /* istanbul ignore next */
    (cov_ns6b9lara().s[103]++, this.alerts.find(a => {
      /* istanbul ignore next */
      cov_ns6b9lara().f[36]++;
      cov_ns6b9lara().s[104]++;
      return a.id === alertId;
    }));
    /* istanbul ignore next */
    cov_ns6b9lara().s[105]++;
    if (alert) {
      /* istanbul ignore next */
      cov_ns6b9lara().b[19][0]++;
      cov_ns6b9lara().s[106]++;
      alert.resolved = true;
      /* istanbul ignore next */
      cov_ns6b9lara().s[107]++;
      alert.resolvedAt = new Date().toISOString();
    } else
    /* istanbul ignore next */
    {
      cov_ns6b9lara().b[19][1]++;
    }
  }
