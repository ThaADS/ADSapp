  }
  startMonitoring() {
    /* istanbul ignore next */
    cov_ns6b9lara().f[10]++;
    cov_ns6b9lara().s[19]++;
    // Check alerts every minute
    setInterval(() => {
      /* istanbul ignore next */
      cov_ns6b9lara().f[11]++;
      cov_ns6b9lara().s[20]++;
      this.checkAlerts();
    }, 60000);
  }
  async checkAlerts() {
    /* istanbul ignore next */
    cov_ns6b9lara().f[12]++;
    cov_ns6b9lara().s[21]++;
    try {
      const metrics =
      /* istanbul ignore next */
      (cov_ns6b9lara().s[22]++, await this.collectMetrics());
      /* istanbul ignore next */
      cov_ns6b9lara().s[23]++;
      for (const rule of this.rules) {
        /* istanbul ignore next */
        cov_ns6b9lara().s[24]++;
        if (!rule.enabled) {
          /* istanbul ignore next */
          cov_ns6b9lara().b[1][0]++;
          cov_ns6b9lara().s[25]++;
          continue;
        } else
        /* istanbul ignore next */
        {
          cov_ns6b9lara().b[1][1]++;
        }
        // Check cooldown
        const lastAlertTime =
        /* istanbul ignore next */
        (cov_ns6b9lara().s[26]++,
        /* istanbul ignore next */
        (cov_ns6b9lara().b[2][0]++, this.lastAlert[rule.id]) ||
        /* istanbul ignore next */
        (cov_ns6b9lara().b[2][1]++, 0));
        const cooldownExpired =
        /* istanbul ignore next */
        (cov_ns6b9lara().s[27]++, Date.now() - lastAlertTime > rule.cooldown * 60 * 1000);
        /* istanbul ignore next */
        cov_ns6b9lara().s[28]++;
        if (!cooldownExpired) {
          /* istanbul ignore next */
          cov_ns6b9lara().b[3][0]++;
          cov_ns6b9lara().s[29]++;
          continue;
        } else
        /* istanbul ignore next */
        {
          cov_ns6b9lara().b[3][1]++;
        }
        // Check condition
        cov_ns6b9lara().s[30]++;
        if (rule.condition(metrics)) {
          /* istanbul ignore next */
          cov_ns6b9lara().b[4][0]++;
          cov_ns6b9lara().s[31]++;
          await this.triggerAlert(rule, metrics);
          /* istanbul ignore next */
          cov_ns6b9lara().s[32]++;
          this.lastAlert[rule.id] = Date.now();
        } else
        /* istanbul ignore next */
        {
          cov_ns6b9lara().b[4][1]++;
        }
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_ns6b9lara().s[33]++;
      console.error('Alert monitoring error:', error);
    }
  }
  async collectMetrics() {
    /* istanbul ignore next */
    cov_ns6b9lara().f[13]++;
    // Collect various metrics from different sources
    const now =
    /* istanbul ignore next */
    (cov_ns6b9lara().s[34]++, Date.now());
    const oneHourAgo =
    /* istanbul ignore next */
    (cov_ns6b9lara().s[35]++, now - 60 * 60 * 1000);
    /* istanbul ignore next */
    cov_ns6b9lara().s[36]++;
    return {
      // Performance metrics
      averageResponseTime: await this.getAverageResponseTime(oneHourAgo, now),
      errorRate: await this.getErrorRate(oneHourAgo, now),
      memoryUsage: this.getMemoryUsage(),
      // System health
      databaseStatus: await this.getDatabaseStatus(),
      redisStatus: await this.getRedisStatus(),
      // Security metrics
      failedLogins: await this.getFailedLogins(oneHourAgo, now),
      suspiciousActivity: await this.getSuspiciousActivity(oneHourAgo, now),
      // Business metrics
      paymentFailures: await this.getPaymentFailures(oneHourAgo, now),
      activeUsers: await this.getActiveUsers(oneHourAgo, now),
      subscriptionCancellations: await this.getSubscriptionCancellations(oneHourAgo, now)
    };
  }
  async triggerAlert(rule, metrics) {
    /* istanbul ignore next */
    cov_ns6b9lara().f[14]++;
    const alert =
    /* istanbul ignore next */
    (cov_ns6b9lara().s[37]++, {
      id: crypto.randomUUID(),
      type: rule.type,
      severity: rule.severity,
      title: rule.name,
      description: rule.description,
      timestamp: new Date().toISOString(),
      resolved: false,
      metadata: {
        ruleId: rule.id,
        metrics: this.sanitizeMetrics(metrics)
      }
    });
    /* istanbul ignore next */
    cov_ns6b9lara().s[38]++;
    this.alerts.push(alert);
    // Send notifications
    /* istanbul ignore next */
    cov_ns6b9lara().s[39]++;
    await this.sendNotifications(alert);
    // Store in database
    /* istanbul ignore next */
    cov_ns6b9lara().s[40]++;
    await this.storeAlert(alert);
    /* istanbul ignore next */
    cov_ns6b9lara().s[41]++;
    console.warn(` Alert triggered: ${alert.title}`, alert);
  }
  sanitizeMetrics(metrics) {
    /* istanbul ignore next */
    cov_ns6b9lara().f[15]++;
    // Remove sensitive data from metrics before storing
    const sanitized =
    /* istanbul ignore next */
    (cov_ns6b9lara().s[42]++, {
      ...metrics
    });
    /* istanbul ignore next */
    cov_ns6b9lara().s[43]++;
    delete sanitized.sensitive;
    /* istanbul ignore next */
    cov_ns6b9lara().s[44]++;
    return sanitized;
  }
  async sendNotifications(alert) {
    /* istanbul ignore next */
    cov_ns6b9lara().f[16]++;
    const notifications =
    /* istanbul ignore next */
    (cov_ns6b9lara().s[45]++, []);
    // Email notifications for critical alerts
    /* istanbul ignore next */
    cov_ns6b9lara().s[46]++;
    if (alert.severity === 'critical') {
      /* istanbul ignore next */
      cov_ns6b9lara().b[5][0]++;
      cov_ns6b9lara().s[47]++;
      notifications.push(this.sendEmailNotification(alert));
    } else
    /* istanbul ignore next */
    {
      cov_ns6b9lara().b[5][1]++;
    }
    // Slack notifications
    cov_ns6b9lara().s[48]++;
    if (
    /* istanbul ignore next */
    (cov_ns6b9lara().b[7][0]++, alert.severity === 'high') ||
    /* istanbul ignore next */
    (cov_ns6b9lara().b[7][1]++, alert.severity === 'critical')) {
      /* istanbul ignore next */
      cov_ns6b9lara().b[6][0]++;
      cov_ns6b9lara().s[49]++;
      notifications.push(this.sendSlackNotification(alert));
    } else
    /* istanbul ignore next */
    {
      cov_ns6b9lara().b[6][1]++;
    }
    // SMS for critical production issues
    cov_ns6b9lara().s[50]++;
    if (
    /* istanbul ignore next */
    (cov_ns6b9lara().b[9][0]++, alert.severity === 'critical') &&
    /* istanbul ignore next */
    (cov_ns6b9lara().b[9][1]++, process.env.NODE_ENV === 'production')) {
      /* istanbul ignore next */
      cov_ns6b9lara().b[8][0]++;
      cov_ns6b9lara().s[51]++;
      notifications.push(this.sendSMSNotification(alert));
    } else
    /* istanbul ignore next */
    {
      cov_ns6b9lara().b[8][1]++;
    }
    // Discord webhook
    cov_ns6b9lara().s[52]++;
    notifications.push(this.sendDiscordNotification(alert));
    /* istanbul ignore next */
    cov_ns6b9lara().s[53]++;
    await Promise.allSettled(notifications);
  }
