  updateRule(ruleId, updates) {
    /* istanbul ignore next */
    cov_ns6b9lara().f[38]++;
    const rule =
    /* istanbul ignore next */
    (cov_ns6b9lara().s[109]++, this.rules.find(r => {
      /* istanbul ignore next */
      cov_ns6b9lara().f[39]++;
      cov_ns6b9lara().s[110]++;
      return r.id === ruleId;
    }));
    /* istanbul ignore next */
    cov_ns6b9lara().s[111]++;
    if (rule) {
      /* istanbul ignore next */
      cov_ns6b9lara().b[20][0]++;
