      condition: metrics => {
        /* istanbul ignore next */
        cov_ns6b9lara().f[5]++;
        cov_ns6b9lara().s[14]++;
        return metrics.errorRate > 0.05;
      }
    }, {
      id: 'memory-usage-high',
      name: 'High Memory Usage',
      type: 'performance',
      severity: 'medium',
      description: 'Memory usage is above 85%',
      enabled: true,
      cooldown: 10,
      condition: metrics => {
        /* istanbul ignore next */
        cov_ns6b9lara().f[6]++;
        cov_ns6b9lara().s[15]++;
        return metrics.memoryUsage > 0.85;
      }
    }, {
      id: 'database-connection-failure',
      name: 'Database Connection Failure',
      type: 'error',
      severity: 'critical',
      description: 'Database connection has failed',
      enabled: true,
      cooldown: 1,
      condition: metrics => {
        /* istanbul ignore next */
        cov_ns6b9lara().f[7]++;
        cov_ns6b9lara().s[16]++;
        return metrics.databaseStatus === 'down';
      }
