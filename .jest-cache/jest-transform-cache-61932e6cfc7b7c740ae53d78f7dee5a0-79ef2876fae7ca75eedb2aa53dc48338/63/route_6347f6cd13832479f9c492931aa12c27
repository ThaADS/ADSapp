  const middlewareResponse =
  /* istanbul ignore next */
  (cov_1ypw9r2zxv().s[20]++, await (0, _middleware.adminMiddleware)(request));
  /* istanbul ignore next */
  cov_1ypw9r2zxv().s[21]++;
  if (middlewareResponse) {
    /* istanbul ignore next */
    cov_1ypw9r2zxv().b[4][0]++;
    cov_1ypw9r2zxv().s[22]++;
    return middlewareResponse;
  } else
  /* istanbul ignore next */
  {
    cov_1ypw9r2zxv().b[4][1]++;
  }
  cov_1ypw9r2zxv().s[23]++;
  try {
    const {
      key,
      value,
      description
    } =
    /* istanbul ignore next */
    (cov_1ypw9r2zxv().s[24]++, await request.json());
    /* istanbul ignore next */
    cov_1ypw9r2zxv().s[25]++;
    if (
    /* istanbul ignore next */
    (cov_1ypw9r2zxv().b[6][0]++, !key) ||
    /* istanbul ignore next */
    (cov_1ypw9r2zxv().b[6][1]++, value === undefined)) {
      /* istanbul ignore next */
      cov_1ypw9r2zxv().b[5][0]++;
      cov_1ypw9r2zxv().s[26]++;
      return _server.NextResponse.json({
        error: 'Key and value are required'
      }, {
        status: 400
      });
    } else
    /* istanbul ignore next */
    {
      cov_1ypw9r2zxv().b[5][1]++;
    }
    const success =
    /* istanbul ignore next */
    (cov_1ypw9r2zxv().s[27]++, await (0, _superadmin.updateSystemSetting)(key, value, description));
    /* istanbul ignore next */
    cov_1ypw9r2zxv().s[28]++;
    if (!success) {
      /* istanbul ignore next */
      cov_1ypw9r2zxv().b[7][0]++;
      cov_1ypw9r2zxv().s[29]++;
      return _server.NextResponse.json({
        error: 'Failed to update system setting'
      }, {
        status: 500
      });
    } else
    /* istanbul ignore next */
    {
      cov_1ypw9r2zxv().b[7][1]++;
    }
    cov_1ypw9r2zxv().s[30]++;
    return _server.NextResponse.json({
      success: true
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1ypw9r2zxv().s[31]++;
    console.error('Admin settings update API error:', error);
    /* istanbul ignore next */
    cov_1ypw9r2zxv().s[32]++;
    return _server.NextResponse.json({
      error: 'Failed to update system setting'
    }, {
      status: 500
    });
  }
}
