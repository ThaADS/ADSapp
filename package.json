    "bullmq": "^5.61.0",
    "dagre": "^0.8.5",
    "dompurify": "^3.3.0",
    "ioredis": "^5.8.1",
    "jose": "^6.1.0",
    "jsdom": "^27.0.1",
    "libphonenumber-js": "^1.12.26",
    "lucide-react": "^0.544.0",
    "next": "15.5.9",
    "next-intl": "^4.3.9",
    "openid-client": "^6.8.1",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "reactflow": "^11.11.4",
    "recharts": "^3.3.0",
    "resend": "^6.1.0",
    "stripe": "^18.5.0",
    "uuid": "^9.0.1",
    "xml-crypto": "^6.1.2",
    "xml2js": "^0.6.2",
