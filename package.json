    "eslint-plugin-security": "^3.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lighthouse": "^12.2.1",
    "lint-staged": "^15.2.0",
    "nock": "^13.5.5",
    "playwright": "^1.47.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "supertest": "^7.0.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "web-vitals": "^4.2.4"
  }
