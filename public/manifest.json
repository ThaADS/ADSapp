  "shortcuts": [
    {
      "name": "Inbox",
      "short_name": "Inbox",
      "description": "View your WhatsApp conversations",
      "url": "/dashboard/inbox",
      "icons": [
        {
          "src": "/icons/inbox-shortcut.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Contacts",
      "short_name": "Contacts",
      "description": "Manage your contacts",
      "url": "/dashboard/contacts",
      "icons": [
        {
          "src": "/icons/contacts-shortcut.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "Analytics",
      "short_name": "Analytics",
      "description": "View analytics dashboard",
      "url": "/dashboard/analytics",
      "icons": [
        {
          "src": "/icons/analytics-shortcut.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/desktop-inbox.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Desktop Inbox View"
    },
    {
      "src": "/screenshots/mobile-inbox.png",
      "sizes": "390x844",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Mobile Inbox View"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "media",
          "accept": ["image/*", "video/*", "audio/*", ".pdf", ".doc", ".docx"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "whatsapp",
      "url": "/share?url=%s"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  }
}
